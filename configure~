int gettimeofday (struct timeval *restrict, struct timezone *restrict);

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

  ;
  return 0;
}
_ACEOF
