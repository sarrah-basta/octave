fi

done
    if test $ac_octave_camd_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for camd_postorder in $CAMD_LIBS" >&5
printf %s "checking for camd_postorder in $CAMD_LIBS... " >&6; }
