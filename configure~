   if test $build_qt_graphics = yes; then
  AMCOND_BUILD_QT_GRAPHICS_TRUE=
  AMCOND_BUILD_QT_GRAPHICS_FALSE='#'
else
  AMCOND_BUILD_QT_GRAPHICS_TRUE='#'
  AMCOND_BUILD_QT_GRAPHICS_FALSE=
fi

   if test $use_qscintilla = yes; then
  AMCOND_HAVE_QSCINTILLA_TRUE=
  AMCOND_HAVE_QSCINTILLA_FALSE='#'
else
  AMCOND_HAVE_QSCINTILLA_TRUE='#'
  AMCOND_HAVE_QSCINTILLA_FALSE=
fi

   if test $win32_terminal = yes; then
  WIN32_TERMINAL_TRUE=
  WIN32_TERMINAL_FALSE='#'
else
  WIN32_TERMINAL_TRUE='#'
  WIN32_TERMINAL_FALSE=
fi



## Default terminal font for the GUI.

case $host_os in
  mingw* | msdosmsvc)
    DEFAULT_TERMINAL_FONT="Lucida Console"
  ;;
  *)
    DEFAULT_TERMINAL_FONT="Courier"
  ;;
esac
DEFAULT_TERMINAL_FONT_SIZE=10



### Check for FLTK library

check_fltk=yes
build_fltk_graphics=no


# Check whether --with-fltk was given.
if test ${with_fltk+y}
then :
  withval=$with_fltk; if test x"$withval" = xno; then
     check_fltk=no
   fi
fi


FLTK_CPPFLAGS=
FLTK_LDFLAGS=
FLTK_LIBS=

if test $check_fltk = yes; then

# Check whether --with-fltk-prefix was given.
if test ${with_fltk_prefix+y}
then :
  withval=$with_fltk_prefix; fltk_prefix="$withval"
else $as_nop
  fltk_prefix=""
fi



# Check whether --with-fltk-exec-prefix was given.
if test ${with_fltk_exec_prefix+y}
then :
  withval=$with_fltk_exec_prefix; fltk_exec_prefix="$withval"
else $as_nop
  fltk_exec_prefix=""
fi


  if test "x$fltk_exec_prefix" != x; then
    fltk_args="$fltk_args --exec-prefix=$fltk_exec_prefix"
    if test "x${FLTK_CONFIG+set}" != xset ; then
      FLTK_CONFIG="$fltk_exec_prefix/bin/fltk-config"
    fi
  fi

  if test "x$fltk_prefix" != x; then
    fltk_args="$fltk_args --prefix=$fltk_prefix"
    if test x${FLTK_CONFIG+set} != xset ; then
      FLTK_CONFIG="$fltk_prefix/bin/fltk-config"
    fi
  fi

  # Extract the first word of "fltk-config", so it can be a program name with args.
set dummy fltk-config; ac_word=$2
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
printf %s "checking for $ac_word... " >&6; }
if test ${ac_cv_path_FLTK_CONFIG+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  case $FLTK_CONFIG in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_FLTK_CONFIG="$FLTK_CONFIG" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
    *) as_dir=$as_dir/ ;;
  esac
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir$ac_word$ac_exec_ext"; then
    ac_cv_path_FLTK_CONFIG="$as_dir$ac_word$ac_exec_ext"
    printf "%s\n" "$as_me:${as_lineno-$LINENO}: found $as_dir$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  test -z "$ac_cv_path_FLTK_CONFIG" && ac_cv_path_FLTK_CONFIG="no"
  ;;
esac
fi
FLTK_CONFIG=$ac_cv_path_FLTK_CONFIG
if test -n "$FLTK_CONFIG"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $FLTK_CONFIG" >&5
printf "%s\n" "$FLTK_CONFIG" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
fi



  warn_fltk_config=
  warn_fltk_opengl=

  if test "$FLTK_CONFIG" = no; then
    warn_fltk_config="FLTK config script not found.  FLTK toolkit will be disabled."

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_fltk_config" >&5
printf "%s\n" "$as_me: WARNING: $warn_fltk_config" >&2;}


  else
    ## Only include -I options in FLTK_CPPFLAGS
    fltk_cflags=`$FLTK_CONFIG $fltkconf_args --use-gl --cflags`
    for fltk_option in $fltk_cflags; do
      case $fltk_option in
        -I*)
          FLTK_CPPFLAGS="$FLTK_CPPFLAGS $fltk_option"
        ;;
      esac
    done
    FLTK_CPPFLAGS=`echo "$FLTK_CPPFLAGS" | $SED -e 's/^ \+//'`

    ## Split -L and -l options into FLTK_LDFLAGS and FLTK_LIBS
    fltk_ldflags=`$FLTK_CONFIG $fltkconf_args --use-gl --ldflags`
    for fltk_option in $fltk_ldflags; do
      case $fltk_option in
        -l*)
          FLTK_LIBS="$FLTK_LIBS $fltk_option"
        ;;
        *)
          FLTK_LDFLAGS="$FLTK_LDFLAGS $fltk_option"
        ;;
      esac
    done
    FLTK_LIBS=`echo "$FLTK_LIBS" | $SED -e 's/^ \+//'`
    FLTK_LDFLAGS=`echo "$FLTK_LDFLAGS" | $SED -e 's/^ \+//'`

    case $host_os in
      mingw*)
        FLTK_LDFLAGS=`echo $FLTK_LDFLAGS | $SED -e 's/-mwindows//g'`
      ;;
    esac
    ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for OpenGL support in FLTK" >&5
printf %s "checking for OpenGL support in FLTK... " >&6; }
if test ${octave_cv_fltk_opengl_support+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  save_CPPFLAGS="$CPPFLAGS"
      CPPFLAGS="$CFLAGS $FLTK_CPPFLAGS"
      cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

        #include <FL/gl.h>

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

        int nothing = 0;

  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_compile "$LINENO"
then :
  octave_cv_fltk_opengl_support=yes
else $as_nop
  octave_cv_fltk_opengl_support=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam conftest.$ac_ext
      CPPFLAGS="$save_CPPFLAGS"

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_fltk_opengl_support" >&5
printf "%s\n" "$octave_cv_fltk_opengl_support" >&6; }
    if test $octave_cv_fltk_opengl_support = no; then
      warn_fltk_opengl="FLTK does not have OpenGL support.  OpenGL graphics with FLTK widgets will be disabled."
    else
      build_fltk_graphics=yes

printf "%s\n" "#define HAVE_FLTK 1" >>confdefs.h

    fi
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu


    if test -n "$warn_fltk_opengl"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_fltk_opengl" >&5
printf "%s\n" "$as_me: WARNING: $warn_fltk_opengl" >&2;}


    fi
  fi
fi





### Final determination of whether OpenGL graphics can be built
if test $build_qt_graphics = no && test $build_fltk_graphics = no; then
  opengl_graphics=no
else
  opengl_graphics=yes
fi

## Check for gl2ps which is required for printing with OpenGL graphics.

if test $opengl_graphics = yes; then
         for ac_header in gl2ps.h
do :
  ac_fn_c_check_header_compile "$LINENO" "gl2ps.h" "ac_cv_header_gl2ps_h" "$ac_includes_default"
if test "x$ac_cv_header_gl2ps_h" = xyes
then :
  printf "%s\n" "#define HAVE_GL2PS_H 1" >>confdefs.h
 GL2PS_LIBS="-lgl2ps"
else $as_nop
  warn_gl2ps="gl2ps library not found.  Printing of OpenGL graphics will be disabled."
fi

done

  if test -n "$warn_gl2ps"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_gl2ps" >&5
printf "%s\n" "$as_me: WARNING: $warn_gl2ps" >&2;}


  else
    save_LIBS="$LIBS"
    LIBS="$GL2PS_LIBS $LIBS"
    ac_fn_c_check_func "$LINENO" "gl2psLineJoin" "ac_cv_func_gl2psLineJoin"
if test "x$ac_cv_func_gl2psLineJoin" = xyes
then :
  printf "%s\n" "#define HAVE_GL2PSLINEJOIN 1" >>confdefs.h

fi

    LIBS="$save_LIBS"
  fi


fi

### Check for the qrupdate library

save_LIBS="$LIBS"
LIBS="$LAPACK_LIBS $BLAS_LIBS $FLIBS $LIBS"


# Check whether --with-qrupdate-includedir was given.
if test ${with_qrupdate_includedir+y}
then :
  withval=$with_qrupdate_includedir; QRUPDATE_CPPFLAGS="-I$withval"
fi




# Check whether --with-qrupdate-libdir was given.
if test ${with_qrupdate_libdir+y}
then :
  withval=$with_qrupdate_libdir; QRUPDATE_LDFLAGS="-L$withval"
fi




# Check whether --with-qrupdate was given.
if test ${with_qrupdate+y}
then :
  withval=$with_qrupdate; with_qrupdate=$withval
else $as_nop
  with_qrupdate=yes
fi


  ac_octave_qrupdate_pkg_check=no
  QRUPDATE_LIBS=
  warn_qrupdate="qrupdate not found.  The QR & Cholesky updating functions will be slow."
  case $with_qrupdate in
    no)
      warn_qrupdate=""
         QRUPDATE_LIBS=
    ;;
    yes | "")
      ac_octave_qrupdate_pkg_check=yes
      QRUPDATE_LIBS="-lqrupdate"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      QRUPDATE_LIBS="$with_qrupdate"
    ;;
    *)
      QRUPDATE_LIBS="-l$with_qrupdate"
    ;;
  esac

  if test $ac_octave_qrupdate_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"qrupdate\""; } >&5
  ($PKG_CONFIG --exists --print-errors "qrupdate") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$QRUPDATE_CPPFLAGS"; then
        QRUPDATE_CPPFLAGS="$($PKG_CONFIG --cflags-only-I qrupdate | $SED -e 's/^ *$//')"
      fi
      if test -z "$QRUPDATE_LDFLAGS"; then
        QRUPDATE_LDFLAGS="$($PKG_CONFIG --libs-only-L qrupdate | $SED -e 's/^ *$//')"
      fi
      QRUPDATE_LIBS="$($PKG_CONFIG --libs-only-l qrupdate | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$QRUPDATE_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$QRUPDATE_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$QRUPDATE_LDFLAGS $LDFLAGS"
    LIBS="$QRUPDATE_LIBS $LIBS"
    ac_ext=f
ac_compile='$F77 -c $FFLAGS conftest.$ac_ext >&5'
ac_link='$F77 -o conftest$ac_exeext $FFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_f77_compiler_gnu

    ac_octave_qrupdate_check_for_lib=no
    ac_octave_qrupdate_check_for_lib=yes
    if test $ac_octave_qrupdate_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for sqr1up in $QRUPDATE_LIBS" >&5
printf %s "checking for sqr1up in $QRUPDATE_LIBS... " >&6; }
if test ${octave_cv_lib_qrupdate+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat > conftest.$ac_ext <<_ACEOF
      program main
      call sqr1up
      end
_ACEOF
if ac_fn_f77_try_link "$LINENO"
then :
  octave_cv_lib_qrupdate=yes
else $as_nop
  octave_cv_lib_qrupdate=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_qrupdate" >&5
printf "%s\n" "$octave_cv_lib_qrupdate" >&6; }
      if test "$octave_cv_lib_qrupdate" = yes; then

          warn_qrupdate=

printf "%s\n" "#define HAVE_QRUPDATE 1" >>confdefs.h

      else
        QRUPDATE_LIBS=
      fi
    else
      octave_cv_lib_qrupdate=no
      QRUPDATE_LIBS=
    fi
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_qrupdate=no
  fi



  if test -n "$warn_qrupdate"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_qrupdate" >&5
printf "%s\n" "$as_me: WARNING: $warn_qrupdate" >&2;}


  fi


## Additional check to see if qrupdate lib found supports LU updates
if test -n "$QRUPDATE_LIBS"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for slup1up in $QRUPDATE_LIBS" >&5
printf %s "checking for slup1up in $QRUPDATE_LIBS... " >&6; }
if test ${octave_cv_func_slup1up+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  LIBS="$LIBS $QRUPDATE_LIBS"
    ac_ext=f
ac_compile='$F77 -c $FFLAGS conftest.$ac_ext >&5'
ac_link='$F77 -o conftest$ac_exeext $FFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_f77_compiler_gnu

    cat > conftest.$ac_ext <<_ACEOF
      program main
      call slup1up
      end
_ACEOF
if ac_fn_f77_try_link "$LINENO"
then :
  octave_cv_func_slup1up=yes
else $as_nop
  octave_cv_func_slup1up=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu


fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_func_slup1up" >&5
printf "%s\n" "$octave_cv_func_slup1up" >&6; }
  if test $octave_cv_func_slup1up = yes; then

printf "%s\n" "#define HAVE_QRUPDATE_LUU 1" >>confdefs.h

  fi
fi
LIBS="$save_LIBS"

