else $as_nop
  as_fn_error $? "Missing function required to build Octave" "$LINENO" 5
fi

done

    ## We need this for GetDeviceCaps.  Should we actually check for
    ## the function and library?
    LIBS="-lgdi32 $LIBS"
  ;;
esac

## Windows-specific module used for winqueryreg function
case $host_os in
  msdosmsvc | mingw*)
           for ac_header in psapi.h
do :
