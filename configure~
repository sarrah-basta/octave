printf "%s\n" "no" >&6; }

  fi

  ;;
esac




### Defaults for cross compiling.

if test "$cross_compiling" = yes; then
  BUILD_CC="gcc"
  BUILD_CFLAGS="-O2 -g"
  BUILD_CXX="g++"
  BUILD_CXXFLAGS="-O2 -g -std=c++11"
  BUILD_LDFLAGS=""
  BUILD_EXEEXT=""
else
  BUILD_CC="${CC}"
  BUILD_CFLAGS="${CFLAGS}"
  BUILD_CXX="${CXX}"
  BUILD_CXXFLAGS="${CXXFLAGS}"
  BUILD_LDFLAGS="${LDFLAGS}"
  BUILD_EXEEXT="${EXEEXT}"
fi








### Determine whether to create cross-mkoctfile and other cross-tools.

cross_tools=no
# Check whether --enable-cross-tools was given.
if test ${enable_cross_tools+y}
then :
  enableval=$enable_cross_tools; if test "$enableval" = yes; then cross_tools=yes; fi
fi


if test "$cross_tools" = yes; then
  if test "$cross_compiling" = no; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: ignoring --enable-cross-tools when not cross compiling" >&5
printf "%s\n" "$as_me: WARNING: ignoring --enable-cross-tools when not cross compiling" >&2;}
    cross_tools=no
  fi
fi
 if test $cross_tools = yes; then
  AMCOND_CROSS_TOOLS_TRUE=
  AMCOND_CROSS_TOOLS_FALSE='#'
else
  AMCOND_CROSS_TOOLS_TRUE='#'
  AMCOND_CROSS_TOOLS_FALSE=
fi


### Check compiler characteristics.

## Check if C++ compiler can auto allocate variable sized arrays.

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether C++ supports dynamic auto arrays" >&5
printf %s "checking whether C++ supports dynamic auto arrays... " >&6; }
if test ${octave_cv_cxx_dynamic_auto_arrays+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

        void test(char *);
        int length();
        char x[length()];
        test(x);

  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_compile "$LINENO"
then :
  octave_cv_cxx_dynamic_auto_arrays=yes
else $as_nop
  octave_cv_cxx_dynamic_auto_arrays=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam conftest.$ac_ext
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu


fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_cxx_dynamic_auto_arrays" >&5
printf "%s\n" "$octave_cv_cxx_dynamic_auto_arrays" >&6; }
  if test $octave_cv_cxx_dynamic_auto_arrays = yes; then

printf "%s\n" "#define HAVE_DYNAMIC_AUTO_ARRAYS 1" >>confdefs.h

  fi


## Check that C compiler and libraries support IEEE754 data format.

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for IEEE 754 data format" >&5
printf %s "checking for IEEE 754 data format... " >&6; }
  if test ${octave_cv_ieee754_data_format+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  if test "$cross_compiling" = yes
then :
  octave_cv_ieee754_data_format=yes
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

        int
        main (void)
        {
          typedef union { unsigned char c[8]; double d; } ieeebytes;

          ieeebytes l = {0x1c, 0xbc, 0x6e, 0xf2, 0x54, 0x8b, 0x11, 0x43};
          ieeebytes b = {0x43, 0x11, 0x8b, 0x54, 0xf2, 0x6e, 0xbc, 0x1c};

          return l.d != 1234567891234567.0 && b.d != 1234567891234567.0;
        }

_ACEOF
if ac_fn_c_try_run "$LINENO"
then :
  octave_cv_ieee754_data_format=yes
else $as_nop
  octave_cv_ieee754_data_format=no
fi
rm -f core *.core core.conftest.* gmon.out bb.out conftest$ac_exeext \
  conftest.$ac_objext conftest.beam conftest.$ac_ext
fi


fi

  if test "$cross_compiling" = yes; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_ieee754_data_format assumed for cross compilation" >&5
printf "%s\n" "$octave_cv_ieee754_data_format assumed for cross compilation" >&6; }
  else
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_ieee754_data_format" >&5
printf "%s\n" "$octave_cv_ieee754_data_format" >&6; }
  fi
  if test $octave_cv_ieee754_data_format = yes; then

printf "%s\n" "#define HAVE_IEEE754_DATA_FORMAT 1" >>confdefs.h

  else
    ## If the format is unknown, then you will probably not have a
    ## useful system, so we will abort here.  Anyone wishing to
    ## experiment with building Octave on a system without IEEE
    ## floating point should be capable of removing this check and
    ## the one in the octave_ieee_init function in liboctave/lo-ieee.cc.
    as_fn_error $? "IEEE 754 data format required for building Octave" "$LINENO" 5
  fi


## Are bit_and, bit_or, and bit_xor defined as templated operators?

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether bit_and, bit_or, bit_xor are defined in the C++ library" >&5
printf %s "checking whether bit_and, bit_or, bit_xor are defined in the C++ library... " >&6; }
if test ${octave_cv_cxx_bitwise_op_templates+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  ac_ext=cpp
