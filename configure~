{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_sunmath_acosp" >&5
printf "%s\n" "$ac_cv_lib_sunmath_acosp" >&6; }
if test "x$ac_cv_lib_sunmath_acosp" = xyes
then :
  as_ac_Lib=`printf "%s\n" "ac_cv_lib_sunperf_$sgemm" | $as_tr_sh`
