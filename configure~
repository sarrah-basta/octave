              # include <unistd.h>
              #endif
              #include <time.h>

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
time_t a;
              struct tm r;
              localtime_r (&a, &r);

  ;
  return 0;
}

