"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 have_opengl_incs=yes; break
fi

done
      break

fi

done

    if test $have_opengl_incs = yes; then
      ac_fn_c_check_header_compile "$LINENO" "GL/glext.h" "ac_cv_header_GL_glext_h" "
#if defined (HAVE_WINDOWS_H)
# include <windows.h>
#endif
#if defined (HAVE_GL_GL_H)
# include <GL/gl.h>
#elif defined (HAVE_OPENGL_GL_H)
# include <OpenGL/gl.h>
#endif

"
if test "x$ac_cv_header_GL_glext_h" = xyes
then :
  printf "%s\n" "#define HAVE_GL_GLEXT_H 1" >>confdefs.h

fi
ac_fn_c_check_header_compile "$LINENO" "OpenGL/glext.h" "ac_cv_header_OpenGL_glext_h" "
#if defined (HAVE_WINDOWS_H)
# include <windows.h>
#endif
#if defined (HAVE_GL_GL_H)
# include <GL/gl.h>
#elif defined (HAVE_OPENGL_GL_H)
# include <OpenGL/gl.h>
#endif

"
if test "x$ac_cv_header_OpenGL_glext_h" = xyes
then :
  printf "%s\n" "#define HAVE_OPENGL_GLEXT_H 1" >>confdefs.h

fi

      case $canonical_host_type in
        *-*-mingw32* | *-*-msdosmsvc)
          ac_octave_save_LIBS="$LIBS"
          LIBS="$LIBS -lopengl32"
          { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for glEnable in -lopengl32" >&5
printf %s "checking for glEnable in -lopengl32... " >&6; }
          cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

            #if HAVE_WINDOWS_H
            # include <windows.h>
            #endif
            #if defined (HAVE_GL_GL_H)
            # include <GL/gl.h>
            #elif defined (HAVE_OPENGL_GL_H)
            # include <OpenGL/gl.h>
            #endif

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

            glEnable(GL_SMOOTH);

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  OPENGL_LIBS="-lopengl32 -lglu32"
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

          LIBS="$ac_octave_save_LIBS"
          if test -n "$OPENGL_LIBS"; then
            { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
          else
            { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
          fi
          ;;
        *)
          ## Non-Mac, Non-Windows systems use this check
          { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for glEnable in -lGL" >&5
printf %s "checking for glEnable in -lGL... " >&6; }
if test ${ac_cv_lib_GL_glEnable+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  ac_check_lib_save_LIBS=$LIBS
LIBS="-lGL  $LIBS"
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char glEnable ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return glEnable ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  ac_cv_lib_GL_glEnable=yes
else $as_nop
  ac_cv_lib_GL_glEnable=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_GL_glEnable" >&5
printf "%s\n" "$ac_cv_lib_GL_glEnable" >&6; }
if test "x$ac_cv_lib_GL_glEnable" = xyes
then :
  OPENGL_LIBS="-lGL -lGLU"
fi

          ;;
      esac
    fi
  fi

  if test -n "$OPENGL_LIBS"; then

printf "%s\n" "#define HAVE_OPENGL 1" >>confdefs.h

  fi

  if test -z "$OPENGL_LIBS"; then
    warn_opengl_libs="OpenGL libs (GL and GLU) not found.  OpenGL graphics will be disabled."

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_opengl_libs" >&5
printf "%s\n" "$as_me: WARNING: $warn_opengl_libs" >&2;}


  fi
fi

## Check for FreeType 2 library

check_freetype=yes
warn_freetype=

# Check whether --with-freetype was given.
if test ${with_freetype+y}
then :
  withval=$with_freetype; if test x"$withval" = xno; then
     check_freetype=no
   fi
fi


if test $check_freetype = yes; then

pkg_failed=no
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for freetype2" >&5
printf %s "checking for freetype2... " >&6; }

if test -n "$FT2_CFLAGS"; then
    pkg_cv_FT2_CFLAGS="$FT2_CFLAGS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"freetype2\""; } >&5
  ($PKG_CONFIG --exists --print-errors "freetype2") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  pkg_cv_FT2_CFLAGS=`$PKG_CONFIG --cflags "freetype2" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi
if test -n "$FT2_LIBS"; then
    pkg_cv_FT2_LIBS="$FT2_LIBS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"freetype2\""; } >&5
  ($PKG_CONFIG --exists --print-errors "freetype2") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  pkg_cv_FT2_LIBS=`$PKG_CONFIG --libs "freetype2" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi



if test $pkg_failed = yes; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }

if $PKG_CONFIG --atleast-pkgconfig-version 0.20; then
        _pkg_short_errors_supported=yes
else
        _pkg_short_errors_supported=no
fi
        if test $_pkg_short_errors_supported = yes; then
	        FT2_PKG_ERRORS=`$PKG_CONFIG --short-errors --print-errors --cflags --libs "freetype2" 2>&1`
        else
	        FT2_PKG_ERRORS=`$PKG_CONFIG --print-errors --cflags --libs "freetype2" 2>&1`
        fi
	# Put the nasty error message in config.log where it belongs
	echo "$FT2_PKG_ERRORS" >&5

	warn_freetype="FreeType library not found.  OpenGL graphics will not be fully functional."
elif test $pkg_failed = untried; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
	warn_freetype="FreeType library not found.  OpenGL graphics will not be fully functional."
else
	FT2_CFLAGS=$pkg_cv_FT2_CFLAGS
	FT2_LIBS=$pkg_cv_FT2_LIBS
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }

    min_ft2_version=9.03
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for FreeType2 version >= $min_ft2_version" >&5
printf %s "checking for FreeType2 version >= $min_ft2_version... " >&6; }
    $PKG_CONFIG freetype2 --atleast-version=$min_ft2_version
    ac_status=$?
    if test "$ac_status" = 0; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }

printf "%s\n" "#define HAVE_FREETYPE 1" >>confdefs.h

      save_LIBS="$LIBS"
      LIBS="$FT2_LIBS $LIBS"
      ac_fn_c_check_func "$LINENO" "FT_Reference_Face" "ac_cv_func_FT_Reference_Face"
if test "x$ac_cv_func_FT_Reference_Face" = xyes
then :
  printf "%s\n" "#define HAVE_FT_REFERENCE_FACE 1" >>confdefs.h

fi

      LIBS="$save_LIBS"
    else
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
      warn_freetype="FreeType library >= 9.03 not found.  OpenGL graphics will not be fully functional."
    fi
fi
fi

if test -n "$warn_freetype"; then
  FT2_CFLAGS=
  FT2_LIBS=

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_freetype" >&5
printf "%s\n" "$as_me: WARNING: $warn_freetype" >&2;}


else
      FT2_CPPFLAGS="$FT2_CFLAGS"

fi

## Check for fontconfig library



# Check whether --with-fontconfig-includedir was given.
if test ${with_fontconfig_includedir+y}
then :
  withval=$with_fontconfig_includedir; FONTCONFIG_CPPFLAGS="-I$withval"
fi




# Check whether --with-fontconfig-libdir was given.
if test ${with_fontconfig_libdir+y}
then :
  withval=$with_fontconfig_libdir; FONTCONFIG_LDFLAGS="-L$withval"
fi




# Check whether --with-fontconfig was given.
if test ${with_fontconfig+y}
then :
  withval=$with_fontconfig; with_fontconfig=$withval
else $as_nop
  with_fontconfig=yes
fi


  ac_octave_fontconfig_pkg_check=no
  FONTCONFIG_LIBS=
  warn_fontconfig="Fontconfig library not found.  OpenGL graphics will not be fully functional."
  case $with_fontconfig in
    no)
      warn_fontconfig=""
         FONTCONFIG_LIBS=
    ;;
    yes | "")
      ac_octave_fontconfig_pkg_check=yes
      FONTCONFIG_LIBS="-lfontconfig"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      FONTCONFIG_LIBS="$with_fontconfig"
    ;;
    *)
      FONTCONFIG_LIBS="-l$with_fontconfig"
    ;;
  esac

  if test $ac_octave_fontconfig_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"fontconfig\""; } >&5
  ($PKG_CONFIG --exists --print-errors "fontconfig") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$FONTCONFIG_CPPFLAGS"; then
        FONTCONFIG_CPPFLAGS="$($PKG_CONFIG --cflags-only-I fontconfig | $SED -e 's/^ *$//')"
      fi
      if test -z "$FONTCONFIG_LDFLAGS"; then
        FONTCONFIG_LDFLAGS="$($PKG_CONFIG --libs-only-L fontconfig | $SED -e 's/^ *$//')"
      fi
      FONTCONFIG_LIBS="$($PKG_CONFIG --libs-only-l fontconfig | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$FONTCONFIG_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$FONTCONFIG_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$FONTCONFIG_LDFLAGS $LDFLAGS"
    LIBS="$FONTCONFIG_LIBS $LIBS"

    ac_octave_fontconfig_check_for_lib=no
           for ac_header in fontconfig.h fontconfig/fontconfig.h
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_c_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 ac_octave_fontconfig_check_for_lib=yes; break
fi

done
    if test $ac_octave_fontconfig_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for FcInit in $FONTCONFIG_LIBS" >&5
printf %s "checking for FcInit in $FONTCONFIG_LIBS... " >&6; }
if test ${octave_cv_lib_fontconfig+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char FcInit ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return FcInit ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_fontconfig=yes
else $as_nop
  octave_cv_lib_fontconfig=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_fontconfig" >&5
printf "%s\n" "$octave_cv_lib_fontconfig" >&6; }
      if test "$octave_cv_lib_fontconfig" = yes; then

          warn_fontconfig=

printf "%s\n" "#define HAVE_FONTCONFIG 1" >>confdefs.h

      else
        FONTCONFIG_LIBS=
      fi
    else
      octave_cv_lib_fontconfig=no
      FONTCONFIG_LIBS=
    fi

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_fontconfig=no
  fi



  if test -n "$warn_fontconfig"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_fontconfig" >&5
printf "%s\n" "$as_me: WARNING: $warn_fontconfig" >&2;}


  fi


### GUI/Qt related tests.

QT_VERSIONS="5"


# Check whether --with-qt was given.
if test ${with_qt+y}
then :
  withval=$with_qt; case $withval in
     yes | "")
     ;;
     no)
       QT_VERSIONS=
     ;;
     *)
       QT_VERSIONS="$withval"
     ;;
   esac
fi


check_qscintilla=yes

# Check whether --with-qscintilla was given.
if test ${with_qscintilla+y}
then :
  withval=$with_qscintilla; if test x"$withval" = xno; then
     check_qscintilla=no
   fi
fi




  octave_qt_versions="$QT_VERSIONS"

  build_qt_gui=no
  build_qt_graphics=no
  use_qscintilla=no
  win32_terminal=no

  for ver in $octave_qt_versions; do
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking Qt version $ver" >&5
printf %s "checking Qt version $ver... " >&6; }
  QT_CPPFLAGS=
  QT_LDFLAGS=
  QT_LIBS=

  qt_version="$ver";

  build_qt_gui=yes
  build_qt_graphics=no
  have_qt_opengl_offscreen=no
  win32_terminal=no

  warn_qt_libraries=""
  warn_qt_version=""
  warn_qt_tools=""
  warn_qt_setvbuf=""
  warn_qt_lib_fcns=""
  warn_qt_abstract_item_model=""
  warn_qt_opengl=""

  ## Check for Qt libraries
  case "$qt_version" in
    5)
      QT_OPENGL_MODULE="Qt5OpenGL"
      QT_MODULES="Qt5Core Qt5Gui Qt5Help Qt5Network Qt5PrintSupport Qt5Xml"
    ;;
    *)
      as_fn_error $? "Unrecognized Qt version $qt_version" "$LINENO" 5
    ;;
  esac

  ## Use this check to get info in the log file.

pkg_failed=no
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $QT_MODULES" >&5
printf %s "checking for $QT_MODULES... " >&6; }

if test -n "$QT_CFLAGS"; then
    pkg_cv_QT_CFLAGS="$QT_CFLAGS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"\$QT_MODULES\""; } >&5
  ($PKG_CONFIG --exists --print-errors "$QT_MODULES") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  pkg_cv_QT_CFLAGS=`$PKG_CONFIG --cflags "$QT_MODULES" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi
if test -n "$QT_LIBS"; then
    pkg_cv_QT_LIBS="$QT_LIBS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"\$QT_MODULES\""; } >&5
  ($PKG_CONFIG --exists --print-errors "$QT_MODULES") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  pkg_cv_QT_LIBS=`$PKG_CONFIG --libs "$QT_MODULES" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi



if test $pkg_failed = yes; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }

if $PKG_CONFIG --atleast-pkgconfig-version 0.20; then
        _pkg_short_errors_supported=yes
else
        _pkg_short_errors_supported=no
fi
        if test $_pkg_short_errors_supported = yes; then
	        QT_PKG_ERRORS=`$PKG_CONFIG --short-errors --print-errors --cflags --libs "$QT_MODULES" 2>&1`
        else
	        QT_PKG_ERRORS=`$PKG_CONFIG --print-errors --cflags --libs "$QT_MODULES" 2>&1`
        fi
	# Put the nasty error message in config.log where it belongs
	echo "$QT_PKG_ERRORS" >&5

	build_qt_gui=no
     warn_qt_libraries="Qt libraries not found; disabling Qt GUI"
elif test $pkg_failed = untried; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
	build_qt_gui=no
     warn_qt_libraries="Qt libraries not found; disabling Qt GUI"
else
	QT_CFLAGS=$pkg_cv_QT_CFLAGS
	QT_LIBS=$pkg_cv_QT_LIBS
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }

fi

  ## Check the modules again individually to get lists of modules that
  ## are available and/or missing
  QT_MODULES_AVAILABLE=
  QT_MODULES_MISSING=
  for qt_mod in $QT_MODULES; do
    if $PKG_CONFIG --exists $qt_mod; then
      QT_MODULES_AVAILABLE="$QT_MODULES_AVAILABLE $qt_mod"
    else
      QT_MODULES_MISSING="$QT_MODULES_MISSING $qt_mod"
    fi
  done

  if test $build_qt_gui = yes; then
    ## Retrieve Qt compilation and linker flags
    QT_CPPFLAGS="$($PKG_CONFIG --cflags-only-I $QT_MODULES | $SED -e 's/^ *$//')"
    QT_LDFLAGS="$($PKG_CONFIG --libs-only-L $QT_MODULES | $SED -e 's/^ *$//')"
    QT_LIBS="$($PKG_CONFIG --libs-only-l $QT_MODULES | $SED -e 's/^ *$//')"
    QT_OPENGL_CPPFLAGS="$($PKG_CONFIG --cflags-only-I $QT_OPENGL_MODULE | $SED -e 's/^ *$//')"
    QT_OPENGL_LDFLAGS="$($PKG_CONFIG --libs-only-L $QT_OPENGL_MODULE | $SED -e 's/^ *$//')"
    QT_OPENGL_LIBS="$($PKG_CONFIG --libs-only-l $QT_OPENGL_MODULE | $SED -e 's/^ *$//')"

    case $host_os in
      *darwin*)
        ## Qt might be installed in framework
        if test -z "$QT_LIBS"; then
          QT_LDFLAGS="`$PKG_CONFIG --libs-only-other $QT_MODULES | tr ' ' '\n' | $GREP -e '-F' | uniq | tr '\n' ' '`"
          QT_LIBS="`$PKG_CONFIG --libs-only-other $QT_MODULES | tr ' ' '\n' | $GREP -v -e '-F' | uniq | tr '\n' ' '`"
          QT_OPENGL_LDFLAGS="`$PKG_CONFIG --libs-only-other $QT_OPENGL_MODULE | tr ' ' '\n' | $GREP -e '-F' | uniq | tr '\n' ' '`"
          QT_OPENGL_LIBS="`$PKG_CONFIG --libs-only-other $QT_OPENGL_MODULE | tr ' ' '\n' | $GREP -v -e '-F' | uniq | tr '\n' ' '`"
          ## Enabling link_all_deps works around libtool's imperfect handling
          ## of the -F flag
          if test -n "$QT_LDFLAGS"; then
            link_all_deps=yes
          fi
           if test $link_all_deps = yes; then
  AMCOND_LINK_ALL_DEPS_TRUE=
  AMCOND_LINK_ALL_DEPS_FALSE='#'
else
  AMCOND_LINK_ALL_DEPS_TRUE='#'
  AMCOND_LINK_ALL_DEPS_FALSE=
fi

        fi
      ;;
    esac
  fi

  QT_TOOLS_AVAILABLE=
  QT_TOOLS_MISSING=

  if test $build_qt_gui = yes; then
    if test -n "$ac_tool_prefix"; then
  for ac_prog in qtchooser
  do
    # Extract the first word of "$ac_tool_prefix$ac_prog", so it can be a program name with args.
set dummy $ac_tool_prefix$ac_prog; ac_word=$2
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
printf %s "checking for $ac_word... " >&6; }
if test ${ac_cv_prog_QTCHOOSER+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  if test -n "$QTCHOOSER"; then
  ac_cv_prog_QTCHOOSER="$QTCHOOSER" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
    *) as_dir=$as_dir/ ;;
  esac
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir$ac_word$ac_exec_ext"; then
    ac_cv_prog_QTCHOOSER="$ac_tool_prefix$ac_prog"
    printf "%s\n" "$as_me:${as_lineno-$LINENO}: found $as_dir$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
QTCHOOSER=$ac_cv_prog_QTCHOOSER
if test -n "$QTCHOOSER"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $QTCHOOSER" >&5
printf "%s\n" "$QTCHOOSER" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
