then :
  printf "%s\n" "#define HAVE_SNDFILE_H 1" >>confdefs.h
 ac_octave_sndfile_check_for_lib=yes; break
fi

done
    if test $ac_octave_sndfile_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for sf_open in $SNDFILE_LIBS" >&5
printf %s "checking for sf_open in $SNDFILE_LIBS... " >&6; }
