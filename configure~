        #if defined (HAVE_PCRE_H)
        #  include <pcre.h>
        #elif defined (HAVE_PCRE_PCRE_H)
        #  include <pcre/pcre.h>
        #endif

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

        const char *pattern = "test";
        const char *err;
        int erroffset;
        pcre *data = pcre_compile (pattern, PCRE_UTF8, &err, &erroffset, nullptr);
        return (! data);

  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_run "$LINENO"
then :
  octave_cv_lib_pcre_ok=yes
else $as_nop
  octave_cv_lib_pcre_ok=no
fi
rm -f core *.core core.conftest.* gmon.out bb.out conftest$ac_exeext \
  conftest.$ac_objext conftest.beam conftest.$ac_ext
fi

    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu


fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_pcre_ok" >&5
printf "%s\n" "$octave_cv_lib_pcre_ok" >&6; }
  if test $octave_cv_lib_pcre_ok = yes; then

    :
  else
    as_fn_error $? "PCRE library must be built with UTF support (--enable-utf)" "$LINENO" 5
    :
  fi


      else
        PCRE_LIBS=
      fi
    else
      octave_cv_lib_pcre=no
      PCRE_LIBS=
    fi

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_pcre=no
  fi


    if test $octave_cv_lib_pcre = no; then
      as_fn_error $? "to build Octave, you must have the PCRE library and header files installed" "$LINENO" 5
    fi

  if test -n "$warn_pcre"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_pcre" >&5
printf "%s\n" "$as_me: WARNING: $warn_pcre" >&2;}


  fi


### Check for RapidJSON header only library.

check_for_rapidjson=yes
# Check whether --enable-rapidjson was given.
if test ${enable_rapidjson+y}
then :
  enableval=$enable_rapidjson; if test "$enableval" = no; then check_for_rapidjson=no; fi
fi

warn_rapidjson=
if test $check_for_rapidjson = yes; then
  ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

  ac_fn_cxx_check_header_compile "$LINENO" "rapidjson/rapidjson.h" "ac_cv_header_rapidjson_rapidjson_h" "$ac_includes_default"
if test "x$ac_cv_header_rapidjson_rapidjson_h" = xyes
then :
  have_rapidjson=yes
else $as_nop
  have_rapidjson=no
                   warn_rapidjson="RapidJSON library not found.  Octave will not be able to read or write JSON files."
fi


  if test $have_rapidjson = yes; then

printf "%s\n" "#define HAVE_RAPIDJSON 1" >>confdefs.h


    ## Additional check on RapidJSON library that was found
    ## Some features of the version of RapidJSON that is packaged in many
    ## distributions do not build correctly with newer compilers.
    ## See also:
    ## https://github.com/Tencent/rapidjson/issues/909
    ## https://github.com/Tencent/rapidjson/issues/1718
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for working PrettyWriter in RapidJSON" >&5
printf %s "checking for working PrettyWriter in RapidJSON... " >&6; }
if test ${octave_cv_rapidjson_has_prettywriter+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

        #include <rapidjson/prettywriter.h>

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

        rapidjson::StringBuffer json;
        rapidjson::PrettyWriter<rapidjson::StringBuffer, rapidjson::UTF8<>,
                                rapidjson::UTF8<>, rapidjson::CrtAllocator,
                                rapidjson::kWriteNanAndInfFlag> writer (json);

  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_compile "$LINENO"
then :
  octave_cv_rapidjson_has_prettywriter=yes
else $as_nop
  octave_cv_rapidjson_has_prettywriter=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_rapidjson_has_prettywriter" >&5
printf "%s\n" "$octave_cv_rapidjson_has_prettywriter" >&6; }
    if test $octave_cv_rapidjson_has_prettywriter = yes; then

printf "%s\n" "#define HAVE_RAPIDJSON_PRETTYWRITER 1" >>confdefs.h

    else
      warn_rapidjson='RapidJSON library found, but some features do not seem to work properly.  The "PrettyWriter" option in jsonencode will be disabled.'
    fi
  fi
  ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
