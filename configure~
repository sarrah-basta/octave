  # For backward compatibility's sake, define HAVE_STRERROR_R.
  # (We used to run AC_CHECK_FUNCS_ONCE for strerror_r, as well
  # as AC_CHECK_DECLS_ONCE.)

printf "%s\n" "#define HAVE_STRERROR_R 1" >>confdefs.h

