      fi
      SUNDIALS_NVECSERIAL_LIBS="$($PKG_CONFIG --libs-only-l sundials_nvecserial | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$SUNDIALS_NVECSERIAL_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$SUNDIALS_NVECSERIAL_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$SUNDIALS_NVECSERIAL_LDFLAGS $LDFLAGS"
    LIBS="$SUNDIALS_NVECSERIAL_LIBS $LIBS"

    ac_octave_sundials_nvecserial_check_for_lib=no
           for ac_header in nvector_serial.h nvector/nvector_serial.h
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_c_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
