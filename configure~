int
main (void)
{

         #if defined (HAVE_SUNCONTEXT_CREATE)
           SUNContext *sunContext;
           if (SUNContext_Create (NULL, sunContext) < 0)
             1/0;  // provoke an error
           SUNLinSol_KLU (0, 0, *sunContext);
           SUNContext_Free (sunContext);
         #else
           SUNLinSol_KLU (0, 0);
         #endif

  ;
  return 0;
}
