### Check for the Qhull library.

QHULL_CPPFLAGS=
QHULL_LDFLAGS=
QHULL_LIBS=


# Check whether --with-qhull_r-includedir was given.
if test ${with_qhull_r_includedir+y}
then :
  withval=$with_qhull_r_includedir; QHULL_R_CPPFLAGS="-I$withval"
fi




# Check whether --with-qhull_r-libdir was given.
if test ${with_qhull_r_libdir+y}
then :
  withval=$with_qhull_r_libdir; QHULL_R_LDFLAGS="-L$withval"
fi




# Check whether --with-qhull_r was given.
if test ${with_qhull_r+y}
then :
  withval=$with_qhull_r; with_qhull_r=$withval
else $as_nop
  with_qhull_r=yes
fi


  ac_octave_qhull_r_pkg_check=no
  QHULL_R_LIBS=
  warn_qhull_r="Qhull library not found.  This will result in loss of functionality for some geometry functions."
  case $with_qhull_r in
    no)
      warn_qhull_r=""
         QHULL_R_LIBS=
    ;;
    yes | "")
      ac_octave_qhull_r_pkg_check=yes
      QHULL_R_LIBS="-lqhull_r"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      QHULL_R_LIBS="$with_qhull_r"
    ;;
    *)
      QHULL_R_LIBS="-l$with_qhull_r"
    ;;
  esac

  if test $ac_octave_qhull_r_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"qhull_r\""; } >&5
  ($PKG_CONFIG --exists --print-errors "qhull_r") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$QHULL_R_CPPFLAGS"; then
        QHULL_R_CPPFLAGS="$($PKG_CONFIG --cflags-only-I qhull_r | $SED -e 's/^ *$//')"
      fi
      if test -z "$QHULL_R_LDFLAGS"; then
        QHULL_R_LDFLAGS="$($PKG_CONFIG --libs-only-L qhull_r | $SED -e 's/^ *$//')"
      fi
      QHULL_R_LIBS="$($PKG_CONFIG --libs-only-l qhull_r | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$QHULL_R_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$QHULL_R_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$QHULL_R_LDFLAGS $LDFLAGS"
    LIBS="$QHULL_R_LIBS $LIBS"

    ac_octave_qhull_r_check_for_lib=no
           for ac_header in libqhull_r/libqhull_r.h libqhull_r.h
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_c_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 ac_octave_qhull_r_check_for_lib=yes; break
fi

done
    if test $ac_octave_qhull_r_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for qh_qhull in $QHULL_R_LIBS" >&5
printf %s "checking for qh_qhull in $QHULL_R_LIBS... " >&6; }
if test ${octave_cv_lib_qhull_r+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char qh_qhull ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return qh_qhull ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_qhull_r=yes
else $as_nop
  octave_cv_lib_qhull_r=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_qhull_r" >&5
printf "%s\n" "$octave_cv_lib_qhull_r" >&6; }
      if test "$octave_cv_lib_qhull_r" = yes; then
        warn_qhull_r=

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for qh_version in $QHULL_R_LIBS" >&5
printf %s "checking for qh_version in $QHULL_R_LIBS... " >&6; }
if test ${octave_cv_lib_qhull_r_version+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

        #include <stdio.h>
        #if defined (HAVE_LIBQHULL_R_LIBQHULL_R_H)
        # include <libqhull_r/libqhull_r.h>
        # include <libqhull_r/qset_r.h>
        # include <libqhull_r/geom_r.h>
        # include <libqhull_r/poly_r.h>
        # include <libqhull_r/io_r.h>
        #elif defined (HAVE_LIBQHULL_R_H)
        # include <libqhull_r.h>
        # include <qset_r.h>
        # include <geom_r.h>
        # include <poly_r.h>
        # include <io_r.h>
        #endif

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

        const char *tmp = qh_version;

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_qhull_r_version=yes
else $as_nop
  octave_cv_lib_qhull_r_version=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_qhull_r_version" >&5
printf "%s\n" "$octave_cv_lib_qhull_r_version" >&6; }
  if test $octave_cv_lib_qhull_r_version = no; then

printf "%s\n" "#define NEED_QHULL_R_VERSION 1" >>confdefs.h

  fi


  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether the qhull_r library works" >&5
printf %s "checking whether the qhull_r library works... " >&6; }
if test ${octave_cv_lib_qhull_r_ok+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  if test "$cross_compiling" = yes
then :
  octave_cv_lib_qhull_r_ok=yes
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

        #include <stdio.h>
        #if defined (HAVE_LIBQHULL_R_LIBQHULL_R_H)
        # include <libqhull_r/libqhull_r.h>
        # include <libqhull_r/qset_r.h>
        # include <libqhull_r/geom_r.h>
        # include <libqhull_r/poly_r.h>
        # include <libqhull_r/io_r.h>
        #elif defined (HAVE_LIBQHULL_R_H)
        # include <libqhull_r.h>
        # include <qset_r.h>
        # include <geom_r.h>
        # include <poly_r.h>
        # include <io_r.h>
        #endif
        #if defined (NEED_QHULL_R_VERSION)
          char *qh_version = "version";
        #endif

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

        int dim = 2;
        int n = 4;
        coordT points[8] = { -0.5, -0.5, -0.5, 0.5, 0.5, -0.5, 0.5, 0.5 };
        boolT ismalloc = 0;
        qhT context = { };
        qhT* qh = &context;
        return qh_new_qhull (qh, dim, n, points, ismalloc, "qhull ", 0, stderr);

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_run "$LINENO"
then :
  octave_cv_lib_qhull_r_ok=yes
else $as_nop
  octave_cv_lib_qhull_r_ok=no
fi
rm -f core *.core core.conftest.* gmon.out bb.out conftest$ac_exeext \
  conftest.$ac_objext conftest.beam conftest.$ac_ext
fi


fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_qhull_r_ok" >&5
printf "%s\n" "$octave_cv_lib_qhull_r_ok" >&6; }
  if test $octave_cv_lib_qhull_r_ok = yes; then

printf "%s\n" "#define HAVE_QHULL 1" >>confdefs.h

     QHULL_CPPFLAGS="$QHULL_R_CPPFLAGS"
     QHULL_LDFLAGS="$QHULL_R_LDFLAGS"
     QHULL_LIBS="$QHULL_R_LIBS"
    :
  else
    warn_qhull_r="Qhull library found, but does not seem to work properly.  This will result in loss of functionality for some geometry functions.  Please try recompiling the library with -fno-strict-aliasing."
    :
  fi

      else
        QHULL_R_LIBS=
      fi
    else
      octave_cv_lib_qhull_r=no
      QHULL_R_LIBS=
    fi

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_qhull_r=no
  fi



  if test -n "$warn_qhull_r"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_qhull_r" >&5
printf "%s\n" "$as_me: WARNING: $warn_qhull_r" >&2;}


  fi





### Check for PCRE regex library.



# Check whether --with-pcre-includedir was given.
if test ${with_pcre_includedir+y}
then :
  withval=$with_pcre_includedir; PCRE_CPPFLAGS="-I$withval"
fi




# Check whether --with-pcre-libdir was given.
if test ${with_pcre_libdir+y}
then :
  withval=$with_pcre_libdir; PCRE_LDFLAGS="-L$withval"
fi




# Check whether --with-pcre was given.
if test ${with_pcre+y}
then :
  withval=$with_pcre; with_pcre=$withval
else $as_nop
  with_pcre=yes
fi


  ac_octave_pcre_pkg_check=no
  PCRE_LIBS=
  warn_pcre=""
  case $with_pcre in
    no)
      as_fn_error $? "--without-pcre specified but PCRE is required." "$LINENO" 5
    ;;
    yes | "")
      ac_octave_pcre_pkg_check=yes
      PCRE_LIBS="-lpcre"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      PCRE_LIBS="$with_pcre"
    ;;
    *)
      PCRE_LIBS="-l$with_pcre"
    ;;
  esac

  if test $ac_octave_pcre_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"libpcre\""; } >&5
  ($PKG_CONFIG --exists --print-errors "libpcre") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$PCRE_CPPFLAGS"; then
        PCRE_CPPFLAGS="$($PKG_CONFIG --cflags-only-I libpcre | $SED -e 's/^ *$//')"
      fi
      if test -z "$PCRE_LDFLAGS"; then
        PCRE_LDFLAGS="$($PKG_CONFIG --libs-only-L libpcre | $SED -e 's/^ *$//')"
      fi
      PCRE_LIBS="$($PKG_CONFIG --libs-only-l libpcre | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$PCRE_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$PCRE_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$PCRE_LDFLAGS $LDFLAGS"
    LIBS="$PCRE_LIBS $LIBS"

    ac_octave_pcre_check_for_lib=no
           for ac_header in pcre.h pcre/pcre.h
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_c_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 ac_octave_pcre_check_for_lib=yes; break
fi

done
    if test $ac_octave_pcre_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for pcre_compile in $PCRE_LIBS" >&5
printf %s "checking for pcre_compile in $PCRE_LIBS... " >&6; }
if test ${octave_cv_lib_pcre+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char pcre_compile ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return pcre_compile ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_pcre=yes
else $as_nop
  octave_cv_lib_pcre=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_pcre" >&5
printf "%s\n" "$octave_cv_lib_pcre" >&6; }
      if test "$octave_cv_lib_pcre" = yes; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether PCRE library was compiled with UTF support" >&5
printf %s "checking whether PCRE library was compiled with UTF support... " >&6; }
if test ${octave_cv_lib_pcre_ok+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

    if test "$cross_compiling" = yes
then :
  octave_cv_lib_pcre_ok=yes
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

