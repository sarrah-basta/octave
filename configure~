    LIBS="$SUNDIALS_SUNLINSOLKLU_LIBS $LIBS"

    ac_octave_sundials_sunlinsolklu_check_for_lib=no
    ac_octave_sundials_sunlinsolklu_check_for_lib=yes
    if test $ac_octave_sundials_sunlinsolklu_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for SUNKLU in $SUNDIALS_SUNLINSOLKLU_LIBS" >&5
printf %s "checking for SUNKLU in $SUNDIALS_SUNLINSOLKLU_LIBS... " >&6; }
