    host=unknown-unknown-unknown
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: configuring Octave for unknown system type" >&5
printf "%s\n" "$as_me: WARNING: configuring Octave for unknown system type" >&2;}
  fi
  canonical_host_type=$host

