#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return rl_set_keyboard_input_timeout ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
