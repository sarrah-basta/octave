suitesparse_save_LIBS="$LIBS"
LIBS="$SUITESPARSECONFIG_LIBS $LIBS"

### Check for AMD library



# Check whether --with-amd-includedir was given.
if test ${with_amd_includedir+y}
then :
  withval=$with_amd_includedir; AMD_CPPFLAGS="-I$withval"
fi




# Check whether --with-amd-libdir was given.
if test ${with_amd_libdir+y}
then :
  withval=$with_amd_libdir; AMD_LDFLAGS="-L$withval"
fi




# Check whether --with-amd was given.
if test ${with_amd+y}
then :
  withval=$with_amd; with_amd=$withval
else $as_nop
  with_amd=yes
fi


  ac_octave_amd_pkg_check=no
  AMD_LIBS=
  warn_amd="AMD library not found.  This will result in some lack of functionality for sparse matrices."
  case $with_amd in
    no)
      warn_amd=""
         AMD_LIBS=
    ;;
    yes | "")
      ac_octave_amd_pkg_check=yes
      AMD_LIBS="-lamd"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      AMD_LIBS="$with_amd"
    ;;
    *)
      AMD_LIBS="-l$with_amd"
    ;;
  esac

  if test $ac_octave_amd_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"amd\""; } >&5
  ($PKG_CONFIG --exists --print-errors "amd") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$AMD_CPPFLAGS"; then
        AMD_CPPFLAGS="$($PKG_CONFIG --cflags-only-I amd | $SED -e 's/^ *$//')"
      fi
      if test -z "$AMD_LDFLAGS"; then
        AMD_LDFLAGS="$($PKG_CONFIG --libs-only-L amd | $SED -e 's/^ *$//')"
      fi
      AMD_LIBS="$($PKG_CONFIG --libs-only-l amd | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$AMD_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$AMD_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$AMD_LDFLAGS $LDFLAGS"
    LIBS="$AMD_LIBS $LIBS"

    ac_octave_amd_check_for_lib=no
           for ac_header in amd.h suitesparse/amd.h amd/amd.h
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_c_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 ac_octave_amd_check_for_lib=yes; break
fi

done
    if test $ac_octave_amd_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for amd_postorder in $AMD_LIBS" >&5
printf %s "checking for amd_postorder in $AMD_LIBS... " >&6; }
if test ${octave_cv_lib_amd+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char amd_postorder ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return amd_postorder ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_amd=yes
else $as_nop
  octave_cv_lib_amd=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_amd" >&5
printf "%s\n" "$octave_cv_lib_amd" >&6; }
      if test "$octave_cv_lib_amd" = yes; then

          warn_amd=

printf "%s\n" "#define HAVE_AMD 1" >>confdefs.h

      else
        AMD_LIBS=
      fi
    else
      octave_cv_lib_amd=no
      AMD_LIBS=
    fi

