# ### END LIBTOOL TAG CONFIG: F77
_LT_EOF

 ;;
    "oct-conf-post-private.h":C) $SHELL $srcdir/build-aux/move-if-change oct-conf-post-private.h-tmp oct-conf-post-private.h ;;
    "oct-conf-post-public.h":C) $SHELL $srcdir/build-aux/move-if-change oct-conf-post-public.h-tmp oct-conf-post-public.h ;;
    "liboctave/external/mk-f77-def.sh":C) $SHELL $srcdir/build-aux/move-if-change liboctave/external/mk-f77-def.sh-tmp liboctave/external/mk-f77-def.sh ;;
    "liboctave/mk-version-h.sh":C) $SHELL $srcdir/build-aux/move-if-change liboctave/mk-version-h.sh-tmp liboctave/mk-version-h.sh ;;
    "libinterp/corefcn/mk-mxtypes-h.sh":C) $SHELL $srcdir/build-aux/move-if-change libinterp/corefcn/mk-mxtypes-h.sh-tmp libinterp/corefcn/mk-mxtypes-h.sh ;;
    "build-aux/subst-config-vals.sh":C) $SHELL $srcdir/build-aux/move-if-change build-aux/subst-config-vals.sh-tmp build-aux/subst-config-vals.sh ;;
    "build-aux/subst-cross-config-vals.sh":C) $SHELL $srcdir/build-aux/move-if-change build-aux/subst-cross-config-vals.sh-tmp build-aux/subst-cross-config-vals.sh ;;
    "build-aux/subst-script-vals.sh":C) $SHELL $srcdir/build-aux/move-if-change build-aux/subst-script-vals.sh-tmp build-aux/subst-script-vals.sh ;;

  esac
done # for ac_tag


as_fn_exit 0
_ACEOF
ac_clean_files=$ac_clean_files_save

test $ac_write_fail = 0 ||
  as_fn_error $? "write failure creating $CONFIG_STATUS" "$LINENO" 5


# configure is writing to config.log, and then calls config.status.
# config.status does its own redirection, appending to config.log.
# Unfortunately, on DOS this fails, as config.log is still kept open
# by configure, so config.status won't be able to write to it; its
# output is simply discarded.  So we exec the FD to /dev/null,
# effectively closing config.log, so it can be properly (re)opened and
# appended to by config.status.  When coming back to configure, we
# need to make the FD available again.
if test "$no_create" != yes; then
  ac_cs_success=:
  ac_config_status_args=
  test "$silent" = yes &&
    ac_config_status_args="$ac_config_status_args --quiet"
  exec 5>/dev/null
  $SHELL $CONFIG_STATUS $ac_config_status_args || ac_cs_success=false
  exec 5>>config.log
  # Use ||, not &&, to avoid exiting from the if with $? = 1, which
  # would make configure fail if this is the last instruction.
  $ac_cs_success || as_fn_exit 1
fi
if test -n "$ac_unrecognized_opts" && test "$enable_option_checking" != no; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: unrecognized options: $ac_unrecognized_opts" >&5
printf "%s\n" "$as_me: WARNING: unrecognized options: $ac_unrecognized_opts" >&2;}
fi


### Print a summary of the build configuration.

