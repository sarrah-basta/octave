    no)
      warn_sndfile=""
         SNDFILE_LIBS=
    ;;
    yes | "")
      ac_octave_sndfile_pkg_check=yes
      SNDFILE_LIBS="-lsndfile"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      SNDFILE_LIBS="$with_sndfile"
    ;;
    *)
      SNDFILE_LIBS="-l$with_sndfile"
    ;;
  esac

