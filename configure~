## Check command line for the option to disable multi-threaded FFTW.
build_fftw_threads=yes
# Check whether --enable-fftw-threads was given.
if test ${enable_fftw_threads+y}
then :
  enableval=$enable_fftw_threads; if test "$enableval" = no; then
     build_fftw_threads=no
   fi
fi



 if test -n "$FFTW3_LIBS" && test -n "$FFTW3F_LIBS"; then
  AMCOND_HAVE_FFTW_TRUE=
  AMCOND_HAVE_FFTW_FALSE='#'
else
  AMCOND_HAVE_FFTW_TRUE='#'
  AMCOND_HAVE_FFTW_FALSE=
fi


if test -n "$FFTW3_LIBS" && test -n "$FFTW3F_LIBS"; then

printf "%s\n" "#define HAVE_FFTW 1" >>confdefs.h

else
      build_fftw_threads=no
fi

## Check for the multithreaded FFTW library.
## Fallback to singlethreaded if not found or disabled.
if test $build_fftw_threads = yes; then

  ac_octave_save_CPPFLAGS="$CPPFLAGS"
  ac_octave_save_LDFLAGS="$LDFLAGS"
  ac_octave_save_LIBS="$LIBS"
  CPPFLAGS="$FFTW3_CPPFLAGS $CPPFLAGS"
  LDFLAGS="$FFTW3_LDFLAGS $LDFLAGS"
  LIBS="$FFTW3_LIBS $LIBS"
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for fftw3 multi-threading support" >&5
printf %s "checking for fftw3 multi-threading support... " >&6; }
if test ${octave_cv_fftw3_threads_lib+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
