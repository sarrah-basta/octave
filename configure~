  fi

  if test -n "$SNDFILE_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$SNDFILE_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$SNDFILE_LDFLAGS $LDFLAGS"
    LIBS="$SNDFILE_LIBS $LIBS"

    ac_octave_sndfile_check_for_lib=no
           for ac_header in sndfile.h
do :
  ac_fn_c_check_header_compile "$LINENO" "sndfile.h" "ac_cv_header_sndfile_h" "$ac_includes_default"
