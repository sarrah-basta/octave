### Check for CAMD library



# Check whether --with-camd-includedir was given.
if test ${with_camd_includedir+y}
then :
  withval=$with_camd_includedir; CAMD_CPPFLAGS="-I$withval"
fi




# Check whether --with-camd-libdir was given.
if test ${with_camd_libdir+y}
then :
  withval=$with_camd_libdir; CAMD_LDFLAGS="-L$withval"
fi




# Check whether --with-camd was given.
if test ${with_camd+y}
then :
  withval=$with_camd; with_camd=$withval
else $as_nop
  with_camd=yes
fi


  ac_octave_camd_pkg_check=no
  CAMD_LIBS=
  warn_camd="CAMD library not found.  This will result in some lack of functionality for sparse matrices."
  case $with_camd in
    no)
      warn_camd=""
         CAMD_LIBS=
    ;;
    yes | "")
      ac_octave_camd_pkg_check=yes
      CAMD_LIBS="-lcamd"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      CAMD_LIBS="$with_camd"
    ;;
    *)
      CAMD_LIBS="-l$with_camd"
    ;;
  esac

  if test $ac_octave_camd_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"camd\""; } >&5
  ($PKG_CONFIG --exists --print-errors "camd") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$CAMD_CPPFLAGS"; then
        CAMD_CPPFLAGS="$($PKG_CONFIG --cflags-only-I camd | $SED -e 's/^ *$//')"
      fi
      if test -z "$CAMD_LDFLAGS"; then
        CAMD_LDFLAGS="$($PKG_CONFIG --libs-only-L camd | $SED -e 's/^ *$//')"
      fi
      CAMD_LIBS="$($PKG_CONFIG --libs-only-l camd | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$CAMD_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$CAMD_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$CAMD_LDFLAGS $LDFLAGS"
    LIBS="$CAMD_LIBS $LIBS"

    ac_octave_camd_check_for_lib=no
           for ac_header in camd.h suitesparse/camd.h camd/camd.h
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_c_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
