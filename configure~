ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu


fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_cxx_bitwise_op_templates" >&5
printf "%s\n" "$octave_cv_cxx_bitwise_op_templates" >&6; }
  if test $octave_cv_cxx_bitwise_op_templates = yes; then

printf "%s\n" "#define HAVE_CXX_BITWISE_OP_TEMPLATES 1" >>confdefs.h

  fi


