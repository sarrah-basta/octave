# Check whether --enable-address-sanitizer-flags was given.
if test ${enable_address_sanitizer_flags+y}
then :
  enableval=$enable_address_sanitizer_flags; if test "$enableval" = yes; then
     try_address_sanitizer_flags=yes
   fi
fi


if test $try_address_sanitizer_flags = yes; then
  ADDRESS_SANITIZER_OPTIONS="symbolize=1"

fi

