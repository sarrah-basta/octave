## This should be a colon-separated list of directories.

  : ${localfcnfiledir='${datadir}/octave/site/m'}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: defining localfcnfiledir to be $localfcnfiledir" >&5
printf "%s\n" "defining localfcnfiledir to be $localfcnfiledir" >&6; }




  : ${localapifcnfiledir='${datadir}/octave/site/${api_version}/m'}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: defining localapifcnfiledir to be $localapifcnfiledir" >&5
printf "%s\n" "defining localapifcnfiledir to be $localapifcnfiledir" >&6; }




  : ${localverfcnfiledir='${datadir}/octave/${version}/site/m'}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: defining localverfcnfiledir to be $localverfcnfiledir" >&5
printf "%s\n" "defining localverfcnfiledir to be $localverfcnfiledir" >&6; }



