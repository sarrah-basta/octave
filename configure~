  warn_fftw3f="FFTW3F library not found.  The slower FFTPACK library will be used instead."
  case $with_fftw3f in
    no)
      warn_fftw3f=""
         FFTW3F_LIBS=
    ;;
    yes | "")
      ac_octave_fftw3f_pkg_check=yes
      FFTW3F_LIBS="-lfftw3f"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      FFTW3F_LIBS="$with_fftw3f"
    ;;
    *)
      FFTW3F_LIBS="-l$with_fftw3f"
    ;;
  esac

  if test $ac_octave_fftw3f_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"fftw3f\""; } >&5
  ($PKG_CONFIG --exists --print-errors "fftw3f") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$FFTW3F_CPPFLAGS"; then
        FFTW3F_CPPFLAGS="$($PKG_CONFIG --cflags-only-I fftw3f | $SED -e 's/^ *$//')"
      fi
      if test -z "$FFTW3F_LDFLAGS"; then
        FFTW3F_LDFLAGS="$($PKG_CONFIG --libs-only-L fftw3f | $SED -e 's/^ *$//')"
      fi
      FFTW3F_LIBS="$($PKG_CONFIG --libs-only-l fftw3f | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$FFTW3F_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$FFTW3F_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$FFTW3F_LDFLAGS $LDFLAGS"
    LIBS="$FFTW3F_LIBS $LIBS"

    ac_octave_fftw3f_check_for_lib=no
           for ac_header in fftw3.h
