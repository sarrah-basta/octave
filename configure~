  : ${fcnfiledir='${datadir}/octave/${version}/m'}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: defining fcnfiledir to be $fcnfiledir" >&5
printf "%s\n" "defining fcnfiledir to be $fcnfiledir" >&6; }



