then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 ac_octave_sundials_ida_check_for_lib=yes; break
fi

done
    if test $ac_octave_sundials_ida_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for IDAInit in $SUNDIALS_IDA_LIBS" >&5
printf %s "checking for IDAInit in $SUNDIALS_IDA_LIBS... " >&6; }
