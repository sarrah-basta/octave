    warn_texi2pdf="

I didn't find texi2pdf, but it's only a problem if you need to
reconstruct the PDF version of the manual
"

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_texi2pdf" >&5
printf "%s\n" "$as_me: WARNING: $warn_texi2pdf" >&2;}


  fi




  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for options to make reproducible archives with GNU tar" >&5
printf %s "checking for options to make reproducible archives with GNU tar... " >&6; }
  if test ${octave_cv_tar_flags+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  octave_cv_tar_flags=
        for octave_tar in tar gnutar gtar :; do
      $octave_tar --version >/dev/null 2>&1 && break
    done
        if test x"$octave_tar" != x:; then
      echo > conftest.txt
      for octave_tar_flag in --owner=0 --group=0 --numeric-owner --sort=name; do
        $octave_tar -cf conftest.tar $octave_cv_tar_flags $octave_tar_flag conftest.txt 2>/dev/null
        if test $? -eq 0; then
          octave_cv_tar_flags="${octave_cv_tar_flags:+$octave_cv_tar_flags }$octave_tar_flag"
        fi
      done
      rm -f conftest.tar conftest.txt
    fi

fi


  REPRODUCIBLE_TAR_FLAGS="$octave_cv_tar_flags"

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $REPRODUCIBLE_TAR_FLAGS" >&5
printf "%s\n" "$REPRODUCIBLE_TAR_FLAGS" >&6; }


