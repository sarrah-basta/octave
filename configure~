printf "%s\n" "#define GNULIB_TEST_LINK 1" >>confdefs.h






    LOCALCHARSET_TESTS_ENVIRONMENT=














  if test $HAVE_LOCALE_T = 1; then

    gl_func_newlocale="$ac_cv_func_newlocale"
    gl_func_duplocale="$ac_cv_func_duplocale"
