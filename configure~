      if test -z "$CHOLMOD_CPPFLAGS"; then
        CHOLMOD_CPPFLAGS="$($PKG_CONFIG --cflags-only-I cholmod | $SED -e 's/^ *$//')"
      fi
      if test -z "$CHOLMOD_LDFLAGS"; then
        CHOLMOD_LDFLAGS="$($PKG_CONFIG --libs-only-L cholmod | $SED -e 's/^ *$//')"
      fi
      CHOLMOD_LIBS="$($PKG_CONFIG --libs-only-l cholmod | $SED -e 's/^ *$//')"

