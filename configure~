if test "x$ac_cv_func__getch" = xyes
then :
  printf "%s\n" "#define HAVE__GETCH 1" >>confdefs.h

fi
ac_fn_c_check_func "$LINENO" "_kbhit" "ac_cv_func__kbhit"
if test "x$ac_cv_func__kbhit" = xyes
then :
  printf "%s\n" "#define HAVE__KBHIT 1" >>confdefs.h

fi


## Check for math defines such as M_LN2 in math.h
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for MATH DEFINES in math.h" >&5
printf %s "checking for MATH DEFINES in math.h... " >&6; }
if test ${octave_cv_header_math_defines+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

    #include <math.h>

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

    double x = M_LN2;
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"
then :
  octave_cv_header_math_defines=yes
else $as_nop
  octave_cv_header_math_defines=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_header_math_defines" >&5
printf "%s\n" "$octave_cv_header_math_defines" >&6; }

if test $octave_cv_header_math_defines = no; then
  ## Check again and try defining _USE_MATH_DEFINES
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether _USE_MATH_DEFINES needs to be defined" >&5
printf %s "checking whether _USE_MATH_DEFINES needs to be defined... " >&6; }
if test ${octave_cv_header__use_math_defines+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  save_CPPFLAGS="$CPPFLAGS"
    CPPFLAGS="$CPPFLAGS -D_USE_MATH_DEFINES"
    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

      #include <math.h>

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

      double x = M_LN2;
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"
then :
  octave_cv_header__use_math_defines=yes
else $as_nop
  octave_cv_header__use_math_defines=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam conftest.$ac_ext
    CPPFLAGS="$save_CPPFLAGS"

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_header__use_math_defines" >&5
printf "%s\n" "$octave_cv_header__use_math_defines" >&6; }
  if test $octave_cv_header__use_math_defines = yes; then
    octave_cv_header_math_defines=yes

printf "%s\n" "#define _USE_MATH_DEFINES 1" >>confdefs.h

    CPPFLAGS="$CPPFLAGS -D_USE_MATH_DEFINES"
  fi
fi

if test $octave_cv_header_math_defines = yes; then

printf "%s\n" "#define HAVE_MATH_DEFINES 1" >>confdefs.h

else
  as_fn_error $? "MATH DEFINES in math.h such as M_PI are required to build Octave" "$LINENO" 5
fi

