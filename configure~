if test $have_termios_h != yes \
    && test $have_termio_h != yes \
    && test $have_sgtty_h != yes; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: couldn't find one of termios.h, termio.h, or sgtty.h!" >&5
