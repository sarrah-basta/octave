{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_cholmod" >&5
printf "%s\n" "$octave_cv_lib_cholmod" >&6; }
      if test "$octave_cv_lib_cholmod" = yes; then

          warn_cholmod=

printf "%s\n" "#define HAVE_CHOLMOD 1" >>confdefs.h

      else
        CHOLMOD_LIBS=
      fi
    else
      octave_cv_lib_cholmod=no
      CHOLMOD_LIBS=
    fi

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_cholmod=no
  fi



  if test -n "$warn_cholmod"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_cholmod" >&5
printf "%s\n" "$as_me: WARNING: $warn_cholmod" >&2;}


  fi

LIBS="$save_LIBS"

### Check for SPQR library

save_LIBS="$LIBS"
LIBS="$CHOLMOD_LIBS $LIBS"


# Check whether --with-spqr-includedir was given.
if test ${with_spqr_includedir+y}
then :
  withval=$with_spqr_includedir; SPQR_CPPFLAGS="-I$withval"
fi




# Check whether --with-spqr-libdir was given.
if test ${with_spqr_libdir+y}
then :
  withval=$with_spqr_libdir; SPQR_LDFLAGS="-L$withval"
fi




# Check whether --with-spqr was given.
if test ${with_spqr+y}
then :
  withval=$with_spqr; with_spqr=$withval
else $as_nop
  with_spqr=yes
fi


  ac_octave_spqr_pkg_check=no
  SPQR_LIBS=
  warn_spqr="SPQR library not found.  This will result in some lack of functionality for sparse matrices."
  case $with_spqr in
    no)
      warn_spqr=""
         SPQR_LIBS=
    ;;
    yes | "")
      ac_octave_spqr_pkg_check=yes
      SPQR_LIBS="-lspqr"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      SPQR_LIBS="$with_spqr"
    ;;
    *)
      SPQR_LIBS="-l$with_spqr"
    ;;
  esac

  if test $ac_octave_spqr_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"spqr\""; } >&5
  ($PKG_CONFIG --exists --print-errors "spqr") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$SPQR_CPPFLAGS"; then
        SPQR_CPPFLAGS="$($PKG_CONFIG --cflags-only-I spqr | $SED -e 's/^ *$//')"
      fi
      if test -z "$SPQR_LDFLAGS"; then
        SPQR_LDFLAGS="$($PKG_CONFIG --libs-only-L spqr | $SED -e 's/^ *$//')"
      fi
      SPQR_LIBS="$($PKG_CONFIG --libs-only-l spqr | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$SPQR_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$SPQR_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$SPQR_LDFLAGS $LDFLAGS"
    LIBS="$SPQR_LIBS $LIBS"
    ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

    ac_octave_spqr_check_for_lib=no
           for ac_header in SuiteSparseQR.hpp suitesparse/SuiteSparseQR.hpp
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_cxx_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 ac_octave_spqr_check_for_lib=yes; break
fi

done
    if test $ac_octave_spqr_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for SuiteSparseQR_C in $SPQR_LIBS" >&5
printf %s "checking for SuiteSparseQR_C in $SPQR_LIBS... " >&6; }
if test ${octave_cv_lib_spqr+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

namespace conftest {
  extern "C" int SuiteSparseQR_C ();
}
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return conftest::SuiteSparseQR_C ();
  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_link "$LINENO"
then :
  octave_cv_lib_spqr=yes
else $as_nop
  octave_cv_lib_spqr=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_spqr" >&5
printf "%s\n" "$octave_cv_lib_spqr" >&6; }
      if test "$octave_cv_lib_spqr" = yes; then

          warn_spqr=

printf "%s\n" "#define HAVE_SPQR 1" >>confdefs.h

      else
        SPQR_LIBS=
      fi
    else
      octave_cv_lib_spqr=no
      SPQR_LIBS=
    fi
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_spqr=no
  fi



  if test -n "$warn_spqr"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_spqr" >&5
printf "%s\n" "$as_me: WARNING: $warn_spqr" >&2;}


  fi

LIBS="$save_LIBS"

### Check for CXSparse library



# Check whether --with-cxsparse-includedir was given.
if test ${with_cxsparse_includedir+y}
then :
  withval=$with_cxsparse_includedir; CXSPARSE_CPPFLAGS="-I$withval"
fi




# Check whether --with-cxsparse-libdir was given.
if test ${with_cxsparse_libdir+y}
then :
  withval=$with_cxsparse_libdir; CXSPARSE_LDFLAGS="-L$withval"
fi




# Check whether --with-cxsparse was given.
if test ${with_cxsparse+y}
then :
  withval=$with_cxsparse; with_cxsparse=$withval
else $as_nop
  with_cxsparse=yes
fi


  ac_octave_cxsparse_pkg_check=no
  CXSPARSE_LIBS=
  warn_cxsparse="CXSparse library not found.  This will result in some lack of functionality for sparse matrices."
  case $with_cxsparse in
    no)
      warn_cxsparse=""
         CXSPARSE_LIBS=
    ;;
    yes | "")
      ac_octave_cxsparse_pkg_check=yes
      CXSPARSE_LIBS="-lcxsparse"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      CXSPARSE_LIBS="$with_cxsparse"
    ;;
    *)
      CXSPARSE_LIBS="-l$with_cxsparse"
    ;;
  esac

  if test $ac_octave_cxsparse_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"cxsparse\""; } >&5
  ($PKG_CONFIG --exists --print-errors "cxsparse") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$CXSPARSE_CPPFLAGS"; then
        CXSPARSE_CPPFLAGS="$($PKG_CONFIG --cflags-only-I cxsparse | $SED -e 's/^ *$//')"
      fi
      if test -z "$CXSPARSE_LDFLAGS"; then
        CXSPARSE_LDFLAGS="$($PKG_CONFIG --libs-only-L cxsparse | $SED -e 's/^ *$//')"
      fi
      CXSPARSE_LIBS="$($PKG_CONFIG --libs-only-l cxsparse | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$CXSPARSE_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$CXSPARSE_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$CXSPARSE_LDFLAGS $LDFLAGS"
    LIBS="$CXSPARSE_LIBS $LIBS"
    ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

    ac_octave_cxsparse_check_for_lib=no
           for ac_header in cs.h suitesparse/cs.h cxsparse/cs.h
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_cxx_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 ac_octave_cxsparse_check_for_lib=yes; break
fi

done
    if test $ac_octave_cxsparse_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for cs${CXSPARSE_TAG}sqr in $CXSPARSE_LIBS" >&5
printf %s "checking for cs${CXSPARSE_TAG}sqr in $CXSPARSE_LIBS... " >&6; }
if test ${octave_cv_lib_cxsparse+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

namespace conftest {
  extern "C" int cs${CXSPARSE_TAG}sqr ();
}
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return conftest::cs${CXSPARSE_TAG}sqr ();
  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_link "$LINENO"
then :
  octave_cv_lib_cxsparse=yes
else $as_nop
  octave_cv_lib_cxsparse=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_cxsparse" >&5
printf "%s\n" "$octave_cv_lib_cxsparse" >&6; }
      if test "$octave_cv_lib_cxsparse" = yes; then

          warn_cxsparse=

printf "%s\n" "#define HAVE_CXSPARSE 1" >>confdefs.h

      else
        CXSPARSE_LIBS=
      fi
    else
      octave_cv_lib_cxsparse=no
      CXSPARSE_LIBS=
    fi
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_cxsparse=no
  fi



  if test -n "$warn_cxsparse"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_cxsparse" >&5
printf "%s\n" "$as_me: WARNING: $warn_cxsparse" >&2;}


  fi


if test -n "$CXSPARSE_LIBS"; then
  ## Additional check for CXSparse version >= 2.2
  if test $octave_cv_lib_cxsparse = yes; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether CXSparse is version 2.2 or later" >&5
printf %s "checking whether CXSparse is version 2.2 or later... " >&6; }
if test ${octave_cv_cxsparse_version_ok+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  ac_octave_save_CPPFLAGS="$CPPFLAGS"
    CPPFLAGS="$CXSPARSE_CPPFLAGS $CPPFLAGS"
    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

        #if defined (HAVE_SUITESPARSE_CS_H)
        #include <suitesparse/cs.h>
        #elif defined (HAVE_CXSPARSE_CS_H)
        #include <cxsparse/cs.h>
        #elif defined (HAVE_CS_H)
        #include <cs.h>
        #endif

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

        #if (defined (HAVE_CXSPARSE) \
             && (! defined (CS_VER) \
                 || CS_VER < 2 \
                 || (CS_VER == 2 && CS_SUBVER < 2)))
        #error "Octave requires CXSparse version 2.2 or later"
        #endif

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_cpp "$LINENO"
then :
  octave_cv_cxsparse_version_ok=yes
else $as_nop
  octave_cv_cxsparse_version_ok=no
fi
rm -f conftest.err conftest.i conftest.$ac_ext
    CPPFLAGS="$ac_octave_save_CPPFLAGS"

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_cxsparse_version_ok" >&5
printf "%s\n" "$octave_cv_cxsparse_version_ok" >&6; }
  if test $octave_cv_cxsparse_version_ok = yes; then

printf "%s\n" "#define HAVE_CXSPARSE_VERSION_OK 1" >>confdefs.h

  fi

    if test $octave_cv_cxsparse_version_ok = no; then
      as_fn_error $? "CXSparse library is too old (< version 2.2).  Upgrade CXSparse (SuiteSparse) or configure Octave with --without-cxsparse\"" "$LINENO" 5
    fi
  fi
fi

### Check for UMFPACK library.

save_LIBS="$LIBS"
save_CPPFLAGS="$CPPFLAGS"
LIBS="$AMD_LDFLAGS $AMD_LIBS $BLAS_LIBS $FLIBS $LIBS"
CPPFLAGS="$AMD_CPPFLAGS $CPPFLAGS"


# Check whether --with-umfpack-includedir was given.
if test ${with_umfpack_includedir+y}
then :
  withval=$with_umfpack_includedir; UMFPACK_CPPFLAGS="-I$withval"
fi




# Check whether --with-umfpack-libdir was given.
if test ${with_umfpack_libdir+y}
then :
  withval=$with_umfpack_libdir; UMFPACK_LDFLAGS="-L$withval"
fi




# Check whether --with-umfpack was given.
if test ${with_umfpack+y}
then :
  withval=$with_umfpack; with_umfpack=$withval
else $as_nop
  with_umfpack=yes
fi


  ac_octave_umfpack_pkg_check=no
  UMFPACK_LIBS=
  warn_umfpack="UMFPACK not found.  This will result in some lack of functionality for sparse matrices."
  case $with_umfpack in
    no)
      warn_umfpack=""
         UMFPACK_LIBS=
    ;;
    yes | "")
      ac_octave_umfpack_pkg_check=yes
      UMFPACK_LIBS="-lumfpack"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      UMFPACK_LIBS="$with_umfpack"
    ;;
    *)
      UMFPACK_LIBS="-l$with_umfpack"
    ;;
  esac

  if test $ac_octave_umfpack_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"umfpack\""; } >&5
  ($PKG_CONFIG --exists --print-errors "umfpack") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$UMFPACK_CPPFLAGS"; then
        UMFPACK_CPPFLAGS="$($PKG_CONFIG --cflags-only-I umfpack | $SED -e 's/^ *$//')"
      fi
      if test -z "$UMFPACK_LDFLAGS"; then
        UMFPACK_LDFLAGS="$($PKG_CONFIG --libs-only-L umfpack | $SED -e 's/^ *$//')"
      fi
      UMFPACK_LIBS="$($PKG_CONFIG --libs-only-l umfpack | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$UMFPACK_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$UMFPACK_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$UMFPACK_LDFLAGS $LDFLAGS"
    LIBS="$UMFPACK_LIBS $LIBS"

    ac_octave_umfpack_check_for_lib=no
           for ac_header in umfpack.h suitesparse/umfpack.h umfpack/umfpack.h
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_c_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 ac_octave_umfpack_check_for_lib=yes; break
fi

done
    if test $ac_octave_umfpack_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for umfpack${UMFPACK_TAG}get_determinant in $UMFPACK_LIBS" >&5
printf %s "checking for umfpack${UMFPACK_TAG}get_determinant in $UMFPACK_LIBS... " >&6; }
if test ${octave_cv_lib_umfpack+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char umfpack${UMFPACK_TAG}get_determinant ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return umfpack${UMFPACK_TAG}get_determinant ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_umfpack=yes
else $as_nop
  octave_cv_lib_umfpack=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_umfpack" >&5
printf "%s\n" "$octave_cv_lib_umfpack" >&6; }
      if test "$octave_cv_lib_umfpack" = yes; then

          warn_umfpack=

printf "%s\n" "#define HAVE_UMFPACK 1" >>confdefs.h

      else
        UMFPACK_LIBS=
      fi
    else
      octave_cv_lib_umfpack=no
      UMFPACK_LIBS=
    fi

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_umfpack=no
  fi



  if test -n "$warn_umfpack"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_umfpack" >&5
printf "%s\n" "$as_me: WARNING: $warn_umfpack" >&2;}


  fi

CPPFLAGS="$save_CPPFLAGS"
LIBS="$save_LIBS"

if test -z "$UMFPACK_LIBS"; then
  ## Invalidate the cache and try again with -lcblas.
  $as_unset ac_cv_lib_umfpack_umfpack${UMFPACK_TAG}get_determinant
  $as_unset octave_cv_lib_umfpack
  save_LIBS="$LIBS"
  LIBS="-lcblas $AMD_LDFLAGS $AMD_LIBS $BLAS_LIBS $FLIBS $LIBS"


# Check whether --with-umfpack-includedir was given.
if test ${with_umfpack_includedir+y}
then :
  withval=$with_umfpack_includedir; UMFPACK_CPPFLAGS="-I$withval"
fi




# Check whether --with-umfpack-libdir was given.
if test ${with_umfpack_libdir+y}
then :
  withval=$with_umfpack_libdir; UMFPACK_LDFLAGS="-L$withval"
fi




# Check whether --with-umfpack was given.
if test ${with_umfpack+y}
then :
  withval=$with_umfpack; with_umfpack=$withval
else $as_nop
  with_umfpack=yes
fi


  ac_octave_umfpack_pkg_check=no
  UMFPACK_LIBS=
  warn_umfpack="UMFPACK not found.  This will result in some lack of functionality for sparse matrices."
  case $with_umfpack in
    no)
      warn_umfpack=""
         UMFPACK_LIBS=
    ;;
    yes | "")
      ac_octave_umfpack_pkg_check=yes
      UMFPACK_LIBS="-lumfpack"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      UMFPACK_LIBS="$with_umfpack"
    ;;
    *)
      UMFPACK_LIBS="-l$with_umfpack"
    ;;
  esac

  if test $ac_octave_umfpack_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"umfpack\""; } >&5
  ($PKG_CONFIG --exists --print-errors "umfpack") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$UMFPACK_CPPFLAGS"; then
        UMFPACK_CPPFLAGS="$($PKG_CONFIG --cflags-only-I umfpack | $SED -e 's/^ *$//')"
      fi
      if test -z "$UMFPACK_LDFLAGS"; then
        UMFPACK_LDFLAGS="$($PKG_CONFIG --libs-only-L umfpack | $SED -e 's/^ *$//')"
      fi
      UMFPACK_LIBS="$($PKG_CONFIG --libs-only-l umfpack | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$UMFPACK_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$UMFPACK_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$UMFPACK_LDFLAGS $LDFLAGS"
    LIBS="$UMFPACK_LIBS $LIBS"

    ac_octave_umfpack_check_for_lib=no
           for ac_header in umfpack.h suitesparse/umfpack.h umfpack/umfpack.h
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_c_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 ac_octave_umfpack_check_for_lib=yes; break
fi

done
    if test $ac_octave_umfpack_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for umfpack${UMFPACK_TAG}get_determinant in $UMFPACK_LIBS" >&5
printf %s "checking for umfpack${UMFPACK_TAG}get_determinant in $UMFPACK_LIBS... " >&6; }
if test ${octave_cv_lib_umfpack+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char umfpack${UMFPACK_TAG}get_determinant ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return umfpack${UMFPACK_TAG}get_determinant ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_umfpack=yes
else $as_nop
  octave_cv_lib_umfpack=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_umfpack" >&5
printf "%s\n" "$octave_cv_lib_umfpack" >&6; }
      if test "$octave_cv_lib_umfpack" = yes; then

          warn_umfpack=

printf "%s\n" "#define HAVE_UMFPACK 1" >>confdefs.h

      else
        UMFPACK_LIBS=
      fi
    else
      octave_cv_lib_umfpack=no
      UMFPACK_LIBS=
    fi

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_umfpack=no
  fi



  if test -n "$warn_umfpack"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_umfpack" >&5
printf "%s\n" "$as_me: WARNING: $warn_umfpack" >&2;}


  fi

  if test -n "$UMFPACK_LIBS"; then
    UMFPACK_LIBS="$UMFPACK_LIBS -lcblas"
  fi
  LIBS="$save_LIBS"
fi

### Check for KLU library and header.



# Check whether --with-klu-includedir was given.
if test ${with_klu_includedir+y}
then :
  withval=$with_klu_includedir; KLU_CPPFLAGS="-I$withval"
fi




# Check whether --with-klu-libdir was given.
if test ${with_klu_libdir+y}
then :
  withval=$with_klu_libdir; KLU_LDFLAGS="-L$withval"
fi




# Check whether --with-klu was given.
if test ${with_klu+y}
then :
  withval=$with_klu; with_klu=$withval
else $as_nop
  with_klu=yes
fi


  ac_octave_klu_pkg_check=no
  KLU_LIBS=
  warn_klu="KLU library not found.  This will result in some lack of functionality for sparse matrices."
  case $with_klu in
    no)
      warn_klu=""
         KLU_LIBS=
    ;;
    yes | "")
      ac_octave_klu_pkg_check=yes
      KLU_LIBS="-lklu"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      KLU_LIBS="$with_klu"
    ;;
    *)
      KLU_LIBS="-l$with_klu"
    ;;
  esac

  if test $ac_octave_klu_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"klu\""; } >&5
  ($PKG_CONFIG --exists --print-errors "klu") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$KLU_CPPFLAGS"; then
        KLU_CPPFLAGS="$($PKG_CONFIG --cflags-only-I klu | $SED -e 's/^ *$//')"
      fi
      if test -z "$KLU_LDFLAGS"; then
        KLU_LDFLAGS="$($PKG_CONFIG --libs-only-L klu | $SED -e 's/^ *$//')"
      fi
      KLU_LIBS="$($PKG_CONFIG --libs-only-l klu | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$KLU_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$KLU_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$KLU_LDFLAGS $LDFLAGS"
    LIBS="$KLU_LIBS $LIBS"

    ac_octave_klu_check_for_lib=no
           for ac_header in klu.h suitesparse/klu.h klu/klu.h
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_c_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 ac_octave_klu_check_for_lib=yes; break
fi

done
    if test $ac_octave_klu_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for klu_solve in $KLU_LIBS" >&5
printf %s "checking for klu_solve in $KLU_LIBS... " >&6; }
if test ${octave_cv_lib_klu+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char klu_solve ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return klu_solve ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_klu=yes
else $as_nop
  octave_cv_lib_klu=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_klu" >&5
printf "%s\n" "$octave_cv_lib_klu" >&6; }
      if test "$octave_cv_lib_klu" = yes; then

          warn_klu=

