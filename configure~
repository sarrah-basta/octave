## Collections of flags.

## Order matters, at least on some systems (Cygwin, for example).

FFTW_XCPPFLAGS="$FFTW3_CPPFLAGS $FFTW3F_CPPFLAGS"

FFTW_XLDFLAGS="$FFTW3_LDFLAGS $FFTW3F_LDFLAGS"

FFTW_XLIBS="$FFTW3_LIBS $FFTW3F_LIBS"





## Subdirectory of liboctave/external to build if FFTW is not found.
FFT_DIR="fftpack"


### Check for GLPK library and header.

save_CPPFLAGS="$CPPFLAGS"
save_LIBS="$LIBS"
CPPFLAGS="$Z_CPPFLAGS $CPPFLAGS"
LIBS="$Z_LDFLAGS $Z_LIBS $LIBS"


# Check whether --with-glpk-includedir was given.
if test ${with_glpk_includedir+y}
then :
  withval=$with_glpk_includedir; GLPK_CPPFLAGS="-I$withval"
fi




# Check whether --with-glpk-libdir was given.
if test ${with_glpk_libdir+y}
then :
  withval=$with_glpk_libdir; GLPK_LDFLAGS="-L$withval"
fi




# Check whether --with-glpk was given.
if test ${with_glpk+y}
then :
  withval=$with_glpk; with_glpk=$withval
else $as_nop
  with_glpk=yes
fi


  ac_octave_glpk_pkg_check=no
  GLPK_LIBS=
  warn_glpk="GLPK library not found.  The glpk function for solving linear programs will be disabled."
  case $with_glpk in
    no)
      warn_glpk=""
         GLPK_LIBS=
    ;;
    yes | "")
      ac_octave_glpk_pkg_check=yes
      GLPK_LIBS="-lglpk"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      GLPK_LIBS="$with_glpk"
    ;;
    *)
      GLPK_LIBS="-l$with_glpk"
    ;;
  esac

  if test $ac_octave_glpk_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"glpk\""; } >&5
  ($PKG_CONFIG --exists --print-errors "glpk") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$GLPK_CPPFLAGS"; then
        GLPK_CPPFLAGS="$($PKG_CONFIG --cflags-only-I glpk | $SED -e 's/^ *$//')"
      fi
      if test -z "$GLPK_LDFLAGS"; then
        GLPK_LDFLAGS="$($PKG_CONFIG --libs-only-L glpk | $SED -e 's/^ *$//')"
      fi
      GLPK_LIBS="$($PKG_CONFIG --libs-only-l glpk | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$GLPK_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$GLPK_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$GLPK_LDFLAGS $LDFLAGS"
    LIBS="$GLPK_LIBS $LIBS"

    ac_octave_glpk_check_for_lib=no
           for ac_header in glpk.h glpk/glpk.h
do :
  as_ac_Header=`printf "%s\n" "ac_cv_header_$ac_header" | $as_tr_sh`
ac_fn_c_check_header_compile "$LINENO" "$ac_header" "$as_ac_Header" "$ac_includes_default"
if eval test \"x\$"$as_ac_Header"\" = x"yes"
then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_$ac_header" | $as_tr_cpp` 1
_ACEOF
 ac_octave_glpk_check_for_lib=yes; break
fi

done
    if test $ac_octave_glpk_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for glp_simplex in $GLPK_LIBS" >&5
printf %s "checking for glp_simplex in $GLPK_LIBS... " >&6; }
if test ${octave_cv_lib_glpk+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char glp_simplex ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return glp_simplex ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_glpk=yes
else $as_nop
  octave_cv_lib_glpk=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_glpk" >&5
printf "%s\n" "$octave_cv_lib_glpk" >&6; }
      if test "$octave_cv_lib_glpk" = yes; then
        warn_glpk=

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether the glpk library has glp_interior(glp_prob*, glp_iptcp*)" >&5
printf %s "checking whether the glpk library has glp_interior(glp_prob*, glp_iptcp*)... " >&6; }
if test ${octave_cv_lib_glpk_ok+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

        extern "C"
        {
        #if defined (HAVE_GLPK_GLPK_H)
        #include <glpk/glpk.h>
        #else
        #include <glpk.h>
        #endif
        }

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

        glp_prob *lp = glp_create_prob ();
        glp_iptcp iptcp;
        glp_init_iptcp (&iptcp);
        int retval = glp_interior (lp, &iptcp);

  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_link "$LINENO"
then :
  octave_cv_lib_glpk_ok=yes
else $as_nop
  octave_cv_lib_glpk_ok=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu


fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_glpk_ok" >&5
printf "%s\n" "$octave_cv_lib_glpk_ok" >&6; }
  if test $octave_cv_lib_glpk_ok = yes; then

printf "%s\n" "#define HAVE_GLPK 1" >>confdefs.h

    :
  else
    warn_glpk="GLPK library found, but does not seem to work properly; disabling glpk function"
    :
  fi

      else
        GLPK_LIBS=
      fi
    else
      octave_cv_lib_glpk=no
      GLPK_LIBS=
    fi

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_glpk=no
  fi



  if test -n "$warn_glpk"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_glpk" >&5
printf "%s\n" "$as_me: WARNING: $warn_glpk" >&2;}


  fi

LIBS="$save_LIBS"
CPPFLAGS="$save_CPPFLAGS"

### Checks for cURL header and library.

save_CPPFLAGS="$CPPFLAGS"
save_LIBS="$LIBS"
CPPFLAGS="$Z_CPPFLAGS $CPPFLAGS"
LIBS="$Z_LDFLAGS $Z_LIBS $LIBS"


# Check whether --with-curl-includedir was given.
if test ${with_curl_includedir+y}
then :
  withval=$with_curl_includedir; CURL_CPPFLAGS="-I$withval"
fi




# Check whether --with-curl-libdir was given.
if test ${with_curl_libdir+y}
then :
  withval=$with_curl_libdir; CURL_LDFLAGS="-L$withval"
fi




# Check whether --with-curl was given.
if test ${with_curl+y}
then :
  withval=$with_curl; with_curl=$withval
else $as_nop
  with_curl=yes
fi


  ac_octave_curl_pkg_check=no
  CURL_LIBS=
  warn_curl="cURL library not found.  The ftp objects, urlread, and urlwrite functions will be disabled."
  case $with_curl in
    no)
      warn_curl=""
         CURL_LIBS=
    ;;
    yes | "")
      ac_octave_curl_pkg_check=yes
      CURL_LIBS="-lcurl"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      CURL_LIBS="$with_curl"
    ;;
    *)
      CURL_LIBS="-l$with_curl"
    ;;
  esac

  if test $ac_octave_curl_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"curl\""; } >&5
  ($PKG_CONFIG --exists --print-errors "curl") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$CURL_CPPFLAGS"; then
        CURL_CPPFLAGS="$($PKG_CONFIG --cflags-only-I curl | $SED -e 's/^ *$//')"
      fi
      if test -z "$CURL_LDFLAGS"; then
        CURL_LDFLAGS="$($PKG_CONFIG --libs-only-L curl | $SED -e 's/^ *$//')"
      fi
      CURL_LIBS="$($PKG_CONFIG --libs-only-l curl | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$CURL_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$CURL_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$CURL_LDFLAGS $LDFLAGS"
    LIBS="$CURL_LIBS $LIBS"

    ac_octave_curl_check_for_lib=no
           for ac_header in curl/curl.h
do :
  ac_fn_c_check_header_compile "$LINENO" "curl/curl.h" "ac_cv_header_curl_curl_h" "$ac_includes_default"
if test "x$ac_cv_header_curl_curl_h" = xyes
then :
  printf "%s\n" "#define HAVE_CURL_CURL_H 1" >>confdefs.h
 ac_octave_curl_check_for_lib=yes; break
fi

done
    if test $ac_octave_curl_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for curl_easy_escape in $CURL_LIBS" >&5
printf %s "checking for curl_easy_escape in $CURL_LIBS... " >&6; }
if test ${octave_cv_lib_curl+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char curl_easy_escape ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return curl_easy_escape ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_curl=yes
else $as_nop
  octave_cv_lib_curl=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_curl" >&5
printf "%s\n" "$octave_cv_lib_curl" >&6; }
      if test "$octave_cv_lib_curl" = yes; then

          warn_curl=

printf "%s\n" "#define HAVE_CURL 1" >>confdefs.h

      else
        CURL_LIBS=
      fi
    else
      octave_cv_lib_curl=no
      CURL_LIBS=
    fi

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_curl=no
  fi



  if test -n "$warn_curl"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_curl" >&5
printf "%s\n" "$as_me: WARNING: $warn_curl" >&2;}


  fi

if test -n "$CURL_LIBS"; then
  ## Additional check on cURL library that was found
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for CURLOPT_DIRLISTONLY in curl/curl.h" >&5
printf %s "checking for CURLOPT_DIRLISTONLY in curl/curl.h... " >&6; }
if test ${octave_cv_curl_has_curlopt_dirlistonly+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
