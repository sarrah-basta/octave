## On some systems, explicitly linking with -lsuitesparseconfig is needed
## when using SuiteSparse libraries.  If so, the checks below for specific
## SuiteSparse libraries may fail without it.  Include LIB_CLOCK_GETTIME (from
## gnulib) on systems where needed.

save_LIBS="$LIBS"
LIBS="$LIB_CLOCK_GETTIME $LIBS"


# Check whether --with-suitesparseconfig-includedir was given.
if test ${with_suitesparseconfig_includedir+y}
then :
  withval=$with_suitesparseconfig_includedir; SUITESPARSECONFIG_CPPFLAGS="-I$withval"
fi




# Check whether --with-suitesparseconfig-libdir was given.
if test ${with_suitesparseconfig_libdir+y}
then :
  withval=$with_suitesparseconfig_libdir; SUITESPARSECONFIG_LDFLAGS="-L$withval"
fi




# Check whether --with-suitesparseconfig was given.
if test ${with_suitesparseconfig+y}
then :
  withval=$with_suitesparseconfig; with_suitesparseconfig=$withval
else $as_nop
  with_suitesparseconfig=yes
fi


  ac_octave_suitesparseconfig_pkg_check=no
  SUITESPARSECONFIG_LIBS=
  warn_suitesparseconfig=""
  case $with_suitesparseconfig in
    no)
      warn_suitesparseconfig=""
         SUITESPARSECONFIG_LIBS=
    ;;
    yes | "")
      ac_octave_suitesparseconfig_pkg_check=yes
      SUITESPARSECONFIG_LIBS="-lsuitesparseconfig"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      SUITESPARSECONFIG_LIBS="$with_suitesparseconfig"
    ;;
    *)
      SUITESPARSECONFIG_LIBS="-l$with_suitesparseconfig"
    ;;
  esac

  if test $ac_octave_suitesparseconfig_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"suitesparseconfig\""; } >&5
  ($PKG_CONFIG --exists --print-errors "suitesparseconfig") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$SUITESPARSECONFIG_CPPFLAGS"; then
        SUITESPARSECONFIG_CPPFLAGS="$($PKG_CONFIG --cflags-only-I suitesparseconfig | $SED -e 's/^ *$//')"
      fi
      if test -z "$SUITESPARSECONFIG_LDFLAGS"; then
        SUITESPARSECONFIG_LDFLAGS="$($PKG_CONFIG --libs-only-L suitesparseconfig | $SED -e 's/^ *$//')"
      fi
      SUITESPARSECONFIG_LIBS="$($PKG_CONFIG --libs-only-l suitesparseconfig | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$SUITESPARSECONFIG_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$SUITESPARSECONFIG_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$SUITESPARSECONFIG_LDFLAGS $LDFLAGS"
    LIBS="$SUITESPARSECONFIG_LIBS $LIBS"

    ac_octave_suitesparseconfig_check_for_lib=no
    ac_octave_suitesparseconfig_check_for_lib=yes
    if test $ac_octave_suitesparseconfig_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for SuiteSparse_time in $SUITESPARSECONFIG_LIBS" >&5
printf %s "checking for SuiteSparse_time in $SUITESPARSECONFIG_LIBS... " >&6; }
if test ${octave_cv_lib_suitesparseconfig+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char SuiteSparse_time ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return SuiteSparse_time ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_suitesparseconfig=yes
else $as_nop
  octave_cv_lib_suitesparseconfig=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_suitesparseconfig" >&5
printf "%s\n" "$octave_cv_lib_suitesparseconfig" >&6; }
      if test "$octave_cv_lib_suitesparseconfig" = yes; then

          warn_suitesparseconfig=

printf "%s\n" "#define HAVE_SUITESPARSECONFIG 1" >>confdefs.h

      else
        SUITESPARSECONFIG_LIBS=
      fi
    else
      octave_cv_lib_suitesparseconfig=no
      SUITESPARSECONFIG_LIBS=
    fi

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_suitesparseconfig=no
  fi



  if test -n "$warn_suitesparseconfig"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_suitesparseconfig" >&5
printf "%s\n" "$as_me: WARNING: $warn_suitesparseconfig" >&2;}


  fi

LIBS="$save_LIBS"

if test -n "$SUITESPARSECONFIG_LIBS" && test -n "$LIB_CLOCK_GETTIME"; then
  SUITESPARSECONFIG_LIBS="$SUITESPARSECONFIG_LIBS $LIB_CLOCK_GETTIME"
fi

