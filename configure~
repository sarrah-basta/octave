### Check for PortAudio



# Check whether --with-portaudio-includedir was given.
if test ${with_portaudio_includedir+y}
then :
  withval=$with_portaudio_includedir; PORTAUDIO_CPPFLAGS="-I$withval"
fi




# Check whether --with-portaudio-libdir was given.
if test ${with_portaudio_libdir+y}
then :
  withval=$with_portaudio_libdir; PORTAUDIO_LDFLAGS="-L$withval"
fi




# Check whether --with-portaudio was given.
if test ${with_portaudio+y}
then :
  withval=$with_portaudio; with_portaudio=$withval
else $as_nop
  with_portaudio=yes
fi


  ac_octave_portaudio_pkg_check=no
  PORTAUDIO_LIBS=
  warn_portaudio="PortAudio library not found.  The audioplayer, audiorecorder, and audiodevinfo functions will be disabled."
  case $with_portaudio in
    no)
      warn_portaudio=""
         PORTAUDIO_LIBS=
    ;;
    yes | "")
      ac_octave_portaudio_pkg_check=yes
      PORTAUDIO_LIBS="-lportaudio"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      PORTAUDIO_LIBS="$with_portaudio"
    ;;
    *)
      PORTAUDIO_LIBS="-l$with_portaudio"
    ;;
  esac

  if test $ac_octave_portaudio_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"portaudio-2.0\""; } >&5
  ($PKG_CONFIG --exists --print-errors "portaudio-2.0") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$PORTAUDIO_CPPFLAGS"; then
        PORTAUDIO_CPPFLAGS="$($PKG_CONFIG --cflags-only-I portaudio-2.0 | $SED -e 's/^ *$//')"
      fi
      if test -z "$PORTAUDIO_LDFLAGS"; then
        PORTAUDIO_LDFLAGS="$($PKG_CONFIG --libs-only-L portaudio-2.0 | $SED -e 's/^ *$//')"
      fi
      PORTAUDIO_LIBS="$($PKG_CONFIG --libs-only-l portaudio-2.0 | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$PORTAUDIO_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$PORTAUDIO_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$PORTAUDIO_LDFLAGS $LDFLAGS"
    LIBS="$PORTAUDIO_LIBS $LIBS"

    ac_octave_portaudio_check_for_lib=no
           for ac_header in portaudio.h
do :
  ac_fn_c_check_header_compile "$LINENO" "portaudio.h" "ac_cv_header_portaudio_h" "$ac_includes_default"
if test "x$ac_cv_header_portaudio_h" = xyes
then :
  printf "%s\n" "#define HAVE_PORTAUDIO_H 1" >>confdefs.h
 ac_octave_portaudio_check_for_lib=yes; break
fi

done
    if test $ac_octave_portaudio_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for Pa_GetDeviceCount in $PORTAUDIO_LIBS" >&5
printf %s "checking for Pa_GetDeviceCount in $PORTAUDIO_LIBS... " >&6; }
