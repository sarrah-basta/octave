if test ${enable_command_line_push_parser+y}
then :
  enableval=$enable_command_line_push_parser; if test "$enableval" = no; then ENABLE_COMMAND_LINE_PUSH_PARSER=no; fi
fi

if test $ENABLE_COMMAND_LINE_PUSH_PARSER = yes; then

printf "%s\n" "#define OCTAVE_ENABLE_COMMAND_LINE_PUSH_PARSER 1" >>confdefs.h

fi

### Check for ZLIB library.



# Check whether --with-z-includedir was given.
if test ${with_z_includedir+y}
then :
  withval=$with_z_includedir; Z_CPPFLAGS="-I$withval"
fi




# Check whether --with-z-libdir was given.
if test ${with_z_libdir+y}
then :
  withval=$with_z_libdir; Z_LDFLAGS="-L$withval"
fi




# Check whether --with-z was given.
if test ${with_z+y}
then :
  withval=$with_z; with_z=$withval
else $as_nop
  with_z=yes
fi


  ac_octave_z_pkg_check=no
  Z_LIBS=
  warn_z="ZLIB library not found.  Octave will not be able to load or save compressed data files or HDF5 files."
  case $with_z in
    no)
      warn_z=""
         Z_LIBS=
    ;;
    yes | "")
      ac_octave_z_pkg_check=yes
      Z_LIBS="-lz"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      Z_LIBS="$with_z"
    ;;
    *)
      Z_LIBS="-l$with_z"
    ;;
  esac

  if test $ac_octave_z_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"z\""; } >&5
  ($PKG_CONFIG --exists --print-errors "z") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$Z_CPPFLAGS"; then
        Z_CPPFLAGS="$($PKG_CONFIG --cflags-only-I z | $SED -e 's/^ *$//')"
      fi
      if test -z "$Z_LDFLAGS"; then
        Z_LDFLAGS="$($PKG_CONFIG --libs-only-L z | $SED -e 's/^ *$//')"
      fi
      Z_LIBS="$($PKG_CONFIG --libs-only-l z | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$Z_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$Z_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$Z_LDFLAGS $LDFLAGS"
    LIBS="$Z_LIBS $LIBS"

    ac_octave_z_check_for_lib=no
           for ac_header in zlib.h
do :
  ac_fn_c_check_header_compile "$LINENO" "zlib.h" "ac_cv_header_zlib_h" "$ac_includes_default"
if test "x$ac_cv_header_zlib_h" = xyes
then :
  printf "%s\n" "#define HAVE_ZLIB_H 1" >>confdefs.h
 ac_octave_z_check_for_lib=yes; break
fi

done
    if test $ac_octave_z_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for gzclearerr in $Z_LIBS" >&5
printf %s "checking for gzclearerr in $Z_LIBS... " >&6; }
if test ${octave_cv_lib_z+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char gzclearerr ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return gzclearerr ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_z=yes
else $as_nop
  octave_cv_lib_z=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_z" >&5
printf "%s\n" "$octave_cv_lib_z" >&6; }
      if test "$octave_cv_lib_z" = yes; then

          warn_z=

printf "%s\n" "#define HAVE_Z 1" >>confdefs.h

      else
        Z_LIBS=
      fi
    else
      octave_cv_lib_z=no
      Z_LIBS=
    fi

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_z=no
  fi



  if test -n "$warn_z"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_z" >&5
printf "%s\n" "$as_me: WARNING: $warn_z" >&2;}


  fi


## Also define HAVE_ZLIB if libz is found.
if test -n "$Z_LIBS"; then

printf "%s\n" "#define HAVE_ZLIB 1" >>confdefs.h

fi

### Check for BZIP2 library.



# Check whether --with-bz2-includedir was given.
if test ${with_bz2_includedir+y}
then :
  withval=$with_bz2_includedir; BZ2_CPPFLAGS="-I$withval"
fi




# Check whether --with-bz2-libdir was given.
if test ${with_bz2_libdir+y}
then :
  withval=$with_bz2_libdir; BZ2_LDFLAGS="-L$withval"
fi




# Check whether --with-bz2 was given.
if test ${with_bz2+y}
then :
  withval=$with_bz2; with_bz2=$withval
else $as_nop
  with_bz2=yes
fi


  ac_octave_bz2_pkg_check=no
  BZ2_LIBS=
  warn_bz2="BZIP2 library not found.  Octave will not be able to compress or decompress bzip2 files."
  case $with_bz2 in
    no)
      warn_bz2=""
         BZ2_LIBS=
    ;;
    yes | "")
      ac_octave_bz2_pkg_check=yes
      BZ2_LIBS="-lbz2"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      BZ2_LIBS="$with_bz2"
    ;;
    *)
      BZ2_LIBS="-l$with_bz2"
    ;;
  esac

  if test $ac_octave_bz2_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"bz2\""; } >&5
  ($PKG_CONFIG --exists --print-errors "bz2") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$BZ2_CPPFLAGS"; then
        BZ2_CPPFLAGS="$($PKG_CONFIG --cflags-only-I bz2 | $SED -e 's/^ *$//')"
      fi
      if test -z "$BZ2_LDFLAGS"; then
        BZ2_LDFLAGS="$($PKG_CONFIG --libs-only-L bz2 | $SED -e 's/^ *$//')"
      fi
      BZ2_LIBS="$($PKG_CONFIG --libs-only-l bz2 | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$BZ2_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$BZ2_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$BZ2_LDFLAGS $LDFLAGS"
    LIBS="$BZ2_LIBS $LIBS"

    ac_octave_bz2_check_for_lib=no
           for ac_header in bzlib.h
do :
  ac_fn_c_check_header_compile "$LINENO" "bzlib.h" "ac_cv_header_bzlib_h" "$ac_includes_default"
if test "x$ac_cv_header_bzlib_h" = xyes
then :
  printf "%s\n" "#define HAVE_BZLIB_H 1" >>confdefs.h
 ac_octave_bz2_check_for_lib=yes; break
fi

done
    if test $ac_octave_bz2_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for BZ2_bzCompressInit in $BZ2_LIBS" >&5
printf %s "checking for BZ2_bzCompressInit in $BZ2_LIBS... " >&6; }
if test ${octave_cv_lib_bz2+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char BZ2_bzCompressInit ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return BZ2_bzCompressInit ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_bz2=yes
else $as_nop
  octave_cv_lib_bz2=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_bz2" >&5
printf "%s\n" "$octave_cv_lib_bz2" >&6; }
      if test "$octave_cv_lib_bz2" = yes; then

          warn_bz2=

printf "%s\n" "#define HAVE_BZ2 1" >>confdefs.h

      else
        BZ2_LIBS=
      fi
    else
      octave_cv_lib_bz2=no
      BZ2_LIBS=
    fi

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_bz2=no
  fi



  if test -n "$warn_bz2"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_bz2" >&5
printf "%s\n" "$as_me: WARNING: $warn_bz2" >&2;}


  fi


### Check for HDF5 library.

save_CPPFLAGS="$CPPFLAGS"
save_LIBS="$LIBS"
CPPFLAGS="$Z_CPPFLAGS $CPPFLAGS"
LIBS="$Z_LDFLAGS $Z_LIBS $LIBS"


# Check whether --with-hdf5-includedir was given.
if test ${with_hdf5_includedir+y}
then :
  withval=$with_hdf5_includedir; HDF5_CPPFLAGS="-I$withval"
fi




# Check whether --with-hdf5-libdir was given.
if test ${with_hdf5_libdir+y}
then :
  withval=$with_hdf5_libdir; HDF5_LDFLAGS="-L$withval"
fi




# Check whether --with-hdf5 was given.
if test ${with_hdf5+y}
then :
  withval=$with_hdf5; with_hdf5=$withval
else $as_nop
  with_hdf5=yes
fi


  ac_octave_hdf5_pkg_check=no
  HDF5_LIBS=
  warn_hdf5="HDF5 library not found.  Octave will not be able to load or save HDF5 data files."
  case $with_hdf5 in
    no)
      warn_hdf5=""
         HDF5_LIBS=
    ;;
    yes | "")
      ac_octave_hdf5_pkg_check=yes
      HDF5_LIBS="-lhdf5"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      HDF5_LIBS="$with_hdf5"
    ;;
    *)
      HDF5_LIBS="-l$with_hdf5"
    ;;
  esac

  if test $ac_octave_hdf5_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"hdf5\""; } >&5
  ($PKG_CONFIG --exists --print-errors "hdf5") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$HDF5_CPPFLAGS"; then
        HDF5_CPPFLAGS="$($PKG_CONFIG --cflags-only-I hdf5 | $SED -e 's/^ *$//')"
      fi
      if test -z "$HDF5_LDFLAGS"; then
        HDF5_LDFLAGS="$($PKG_CONFIG --libs-only-L hdf5 | $SED -e 's/^ *$//')"
      fi
      HDF5_LIBS="$($PKG_CONFIG --libs-only-l hdf5 | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$HDF5_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$HDF5_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$HDF5_LDFLAGS $LDFLAGS"
    LIBS="$HDF5_LIBS $LIBS"

    ac_octave_hdf5_check_for_lib=no
           for ac_header in hdf5.h
do :
  ac_fn_c_check_header_compile "$LINENO" "hdf5.h" "ac_cv_header_hdf5_h" "$ac_includes_default"
if test "x$ac_cv_header_hdf5_h" = xyes
then :
  printf "%s\n" "#define HAVE_HDF5_H 1" >>confdefs.h
 ac_octave_hdf5_check_for_lib=yes; break
fi

done
    if test $ac_octave_hdf5_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for H5Gget_num_objs in $HDF5_LIBS" >&5
printf %s "checking for H5Gget_num_objs in $HDF5_LIBS... " >&6; }
if test ${octave_cv_lib_hdf5+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char H5Gget_num_objs ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return H5Gget_num_objs ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_hdf5=yes
else $as_nop
  octave_cv_lib_hdf5=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_hdf5" >&5
printf "%s\n" "$octave_cv_lib_hdf5" >&6; }
      if test "$octave_cv_lib_hdf5" = yes; then
        warn_hdf5=

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether HDF5 library has enforced version 1.6 API" >&5
printf %s "checking whether HDF5 library has enforced version 1.6 API... " >&6; }
if test ${octave_cv_hdf5_has_ver_16_api+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

      #include <hdf5.h>

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

      H5Eset_auto (0, 0);

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_hdf5_has_ver_16_api=yes
else $as_nop
  octave_cv_hdf5_has_ver_16_api=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_hdf5_has_ver_16_api" >&5
printf "%s\n" "$octave_cv_hdf5_has_ver_16_api" >&6; }
  if test $octave_cv_hdf5_has_ver_16_api != yes; then

printf "%s\n" "#define HAVE_HDF5_18 1" >>confdefs.h

  fi


  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether HDF5 library has UTF-8 file API" >&5
printf %s "checking whether HDF5 library has UTF-8 file API... " >&6; }
if test ${octave_cv_hdf5_has_utf8_api+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  case $host_os in
      msdosmsvc | mingw*)
        cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

          #include <stddef.h>
          const wchar_t *H5_get_utf16_str(const char *s);

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

          H5_get_utf16_str ("");

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_hdf5_has_utf8_api=yes
else $as_nop
  octave_cv_hdf5_has_utf8_api=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
      ;;
      *)
        ## Assume yes on all other platforms
        octave_cv_hdf5_has_utf8_api=yes
      ;;
     esac

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_hdf5_has_utf8_api" >&5
printf "%s\n" "$octave_cv_hdf5_has_utf8_api" >&6; }
  if test $octave_cv_hdf5_has_utf8_api = yes; then

printf "%s\n" "#define HAVE_HDF5_UTF8 1" >>confdefs.h

  fi


printf "%s\n" "#define HAVE_HDF5 1" >>confdefs.h

   if test $have_msvc = yes; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking if _HDF5USEDLL_ needs to be defined" >&5
printf %s "checking if _HDF5USEDLL_ needs to be defined... " >&6; }
if test ${octave_cv_lib_hdf5_dll+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

        #include <hdf5.h>

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

        hid_t x = H5T_NATIVE_DOUBLE;
        return x

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_hdf5_dll=no
else $as_nop
  save_CFLAGS="$CFLAGS"
      CFLAGS="$CFLAGS -DWIN32 -D_HDF5USEDLL_"
      ac_octave_save_LIBS="$LIBS"
      LIBS="$HDF5_LIBS $LIBS"
      cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

          #include <hdf5.h>

#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{

          hid_t x = H5T_NATIVE_DOUBLE;
          return x

  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  octave_cv_lib_hdf5_dll=yes
else $as_nop
  octave_cv_lib_hdf5_dll=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
      CFLAGS="$save_CFLAGS"
      LIBS="$ac_octave_save_LIBS"

fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_lib_hdf5_dll" >&5
printf "%s\n" "$octave_cv_lib_hdf5_dll" >&6; }
  if test $octave_cv_lib_hdf5_dll = yes; then

printf "%s\n" "#define _HDF5USEDLL_ 1" >>confdefs.h

  fi

   fi

      else
        HDF5_LIBS=
      fi
    else
      octave_cv_lib_hdf5=no
      HDF5_LIBS=
    fi

    CPPFLAGS="$ac_octave_save_CPPFLAGS"
    LDFLAGS="$ac_octave_save_LDFLAGS"
    LIBS="$ac_octave_save_LIBS"
  else
    octave_cv_lib_hdf5=no
  fi



  if test -n "$warn_hdf5"; then

  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: $warn_hdf5" >&5
printf "%s\n" "$as_me: WARNING: $warn_hdf5" >&2;}


  fi

CPPFLAGS="$save_CPPFLAGS"
LIBS="$save_LIBS"

have_hdf5_int2float_conversions=no
if test $have_hdf5_int2float_conversions = yes; then

printf "%s\n" "#define HAVE_HDF5_INT2FLOAT_CONVERSIONS 1" >>confdefs.h

fi

### Check for FFTW library.
### Default to Fortran FFTPACK if it is not available.

## Check for FFTW header and library.


# Check whether --with-fftw3-includedir was given.
if test ${with_fftw3_includedir+y}
then :
  withval=$with_fftw3_includedir; FFTW3_CPPFLAGS="-I$withval"
fi




# Check whether --with-fftw3-libdir was given.
if test ${with_fftw3_libdir+y}
then :
  withval=$with_fftw3_libdir; FFTW3_LDFLAGS="-L$withval"
fi




# Check whether --with-fftw3 was given.
if test ${with_fftw3+y}
then :
  withval=$with_fftw3; with_fftw3=$withval
else $as_nop
  with_fftw3=yes
fi


  ac_octave_fftw3_pkg_check=no
  FFTW3_LIBS=
  warn_fftw3="FFTW3 library not found.  The slower FFTPACK library will be used instead."
  case $with_fftw3 in
    no)
      warn_fftw3=""
         FFTW3_LIBS=
    ;;
    yes | "")
      ac_octave_fftw3_pkg_check=yes
      FFTW3_LIBS="-lfftw3"
    ;;
    -* | */* | *.a | *.so | *.so.* | *.o)
      FFTW3_LIBS="$with_fftw3"
    ;;
    *)
      FFTW3_LIBS="-l$with_fftw3"
    ;;
  esac

  if test $ac_octave_fftw3_pkg_check = yes; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"fftw3\""; } >&5
  ($PKG_CONFIG --exists --print-errors "fftw3") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then

      if test -z "$FFTW3_CPPFLAGS"; then
        FFTW3_CPPFLAGS="$($PKG_CONFIG --cflags-only-I fftw3 | $SED -e 's/^ *$//')"
      fi
      if test -z "$FFTW3_LDFLAGS"; then
        FFTW3_LDFLAGS="$($PKG_CONFIG --libs-only-L fftw3 | $SED -e 's/^ *$//')"
      fi
      FFTW3_LIBS="$($PKG_CONFIG --libs-only-l fftw3 | $SED -e 's/^ *$//')"

fi
  fi

  if test -n "$FFTW3_LIBS"; then
    ac_octave_save_CPPFLAGS="$CPPFLAGS"
    ac_octave_save_LDFLAGS="$LDFLAGS"
    ac_octave_save_LIBS="$LIBS"
    CPPFLAGS="$FFTW3_CPPFLAGS $CPPFLAGS"
    LDFLAGS="$FFTW3_LDFLAGS $LDFLAGS"
    LIBS="$FFTW3_LIBS $LIBS"

    ac_octave_fftw3_check_for_lib=no
           for ac_header in fftw3.h
do :
  ac_fn_c_check_header_compile "$LINENO" "fftw3.h" "ac_cv_header_fftw3_h" "$ac_includes_default"
if test "x$ac_cv_header_fftw3_h" = xyes
then :
  printf "%s\n" "#define HAVE_FFTW3_H 1" >>confdefs.h
 ac_octave_fftw3_check_for_lib=yes; break
fi

done
    if test $ac_octave_fftw3_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for fftw_plan_dft_1d in $FFTW3_LIBS" >&5
printf %s "checking for fftw_plan_dft_1d in $FFTW3_LIBS... " >&6; }
if test ${octave_cv_lib_fftw3+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char fftw_plan_dft_1d ();
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif
int
main (void)
{
return fftw_plan_dft_1d ();
  ;
  return 0;
}
