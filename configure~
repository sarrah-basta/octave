  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: Creating plots is still possible if gnuplot is installed." >&5
printf "%s\n" "$as_me: WARNING: Creating plots is still possible if gnuplot is installed." >&2;}
  warn_msg_printed=true
fi

if $warn_msg_printed; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: " >&5
printf "%s\n" "$as_me: " >&6;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: NOTE: Libraries or auxiliary programs may be skipped if they are not found" >&5
printf "%s\n" "$as_me: NOTE: Libraries or auxiliary programs may be skipped if they are not found" >&6;}
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: NOTE: OR if they are missing required features on your system." >&5
printf "%s\n" "$as_me: NOTE: OR if they are missing required features on your system." >&6;}
fi

### End of configure.

