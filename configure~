fi

done
    if test $ac_octave_cholmod_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for cholmod${CHOLMOD_TAG}start in $CHOLMOD_LIBS" >&5
printf %s "checking for cholmod${CHOLMOD_TAG}start in $CHOLMOD_LIBS... " >&6; }
