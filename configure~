fi

done
    if test $ac_octave_sundials_nvecserial_check_for_lib = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for N_VNew_Serial in $SUNDIALS_NVECSERIAL_LIBS" >&5
printf %s "checking for N_VNew_Serial in $SUNDIALS_NVECSERIAL_LIBS... " >&6; }
