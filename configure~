/* end confdefs.h.  */

#include <omp.h>

static void
parallel_fill(int * data, int n)
{
  int i;
#pragma omp parallel for
  for (i = 0; i < n; ++i)
    data[i] = i;
}

int
main()
{
  int arr[100000];
  omp_set_num_threads(2);
  parallel_fill(arr, 100000);
  return 0;
}

