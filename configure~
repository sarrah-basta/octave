{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $octave_cv_curl_has_curlopt_dirlistonly" >&5
printf "%s\n" "$octave_cv_curl_has_curlopt_dirlistonly" >&6; }
  if test $octave_cv_curl_has_curlopt_dirlistonly = no; then

printf "%s\n" "#define CURLOPT_DIRLISTONLY CURLOPT_FTPLISTONLY" >>confdefs.h

  fi
