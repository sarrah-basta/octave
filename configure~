  msdosmsvc | mingw*)

printf "%s\n" "#define _UNICODE 1" >>confdefs.h


printf "%s\n" "#define UNICODE 1" >>confdefs.h

  ;;
esac

OCTAVE_UNICODE_EXE_LDFLAGS=""
case $host_os in
  mingw*)
    OCTAVE_UNICODE_EXE_LDFLAGS="-municode"
  ;;
esac



